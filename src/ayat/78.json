[
  {
    "ayat_id": 5673,
    "ayat_number": 1,
    "ayat_arabic": "عَمَّ يَتَسَاۤءَلُوْنَۚ ",
    "ayat_bahasa": "Tentang apakah mereka saling bertanya-tanya?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078001.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5674,
    "ayat_number": 2,
    "ayat_arabic": "عَنِ النَّبَاِ الْعَظِيْمِۙ ",
    "ayat_bahasa": "Tentang berita yang besar (hari kebangkitan),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078002.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5675,
    "ayat_number": 3,
    "ayat_arabic": "الَّذِيْ هُمْ فِيْهِ مُخْتَلِفُوْنَۗ ",
    "ayat_bahasa": "yang dalam hal itu mereka berselisih.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078003.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5676,
    "ayat_number": 4,
    "ayat_arabic": "كَلَّا سَيَعْلَمُوْنَۙ ",
    "ayat_bahasa": "Tidak! Kelak mereka akan mengetahui,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078004.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5677,
    "ayat_number": 5,
    "ayat_arabic": "ثُمَّ كَلَّا سَيَعْلَمُوْنَ",
    "ayat_bahasa": "sekali lagi tidak! Kelak mereka akan mengetahui. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078005.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5678,
    "ayat_number": 6,
    "ayat_arabic": "اَلَمْ نَجْعَلِ الْاَرْضَ مِهٰدًاۙ",
    "ayat_bahasa": "Bukankah Kami telah menjadikan bumi sebagai hamparan, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078006.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5679,
    "ayat_number": 7,
    "ayat_arabic": "وَّالْجِبَالَ اَوْتَادًاۖ",
    "ayat_bahasa": "dan gunung-gunung sebagai pasak?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078007.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5680,
    "ayat_number": 8,
    "ayat_arabic": "وَّخَلَقْنٰكُمْ اَزْوَاجًاۙ ",
    "ayat_bahasa": "Dan Kami menciptakan kamu berpasang-pasangan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078008.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5681,
    "ayat_number": 9,
    "ayat_arabic": "وَّجَعَلْنَا نَوْمَكُمْ سُبَاتًاۙ ",
    "ayat_bahasa": "dan Kami menjadikan tidurmu untuk istirahat,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078009.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5682,
    "ayat_number": 10,
    "ayat_arabic": "وَّجَعَلْنَا الَّيْلَ لِبَاسًاۙ ",
    "ayat_bahasa": "dan Kami menjadikan malam sebagai pakaian,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078010.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5683,
    "ayat_number": 11,
    "ayat_arabic": "وَّجَعَلْنَا النَّهَارَ مَعَاشًاۚ",
    "ayat_bahasa": "dan Kami menjadikan siang untuk mencari penghidupan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078011.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5684,
    "ayat_number": 12,
    "ayat_arabic": "وَبَنَيْنَا فَوْقَكُمْ سَبْعًا شِدَادًاۙ ",
    "ayat_bahasa": "dan Kami membangun di atas kamu tujuh (langit) yang kokoh,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078012.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5685,
    "ayat_number": 13,
    "ayat_arabic": "وَّجَعَلْنَا سِرَاجًا وَّهَّاجًاۖ",
    "ayat_bahasa": "dan Kami menjadikan pelita yang terang-benderang (matahari),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078013.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5686,
    "ayat_number": 14,
    "ayat_arabic": "وَّاَنْزَلْنَا مِنَ الْمُعْصِرٰتِ مَاۤءً ثَجَّاجًاۙ",
    "ayat_bahasa": "dan Kami turunkan dari awan, air hujan yang tercurah dengan hebatnya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078014.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5687,
    "ayat_number": 15,
    "ayat_arabic": "لِّنُخْرِجَ بِهٖ حَبًّا وَّنَبَاتًاۙ",
    "ayat_bahasa": "untuk Kami tumbuhkan dengan air itu biji-bijian dan tanam-tanaman,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078015.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5688,
    "ayat_number": 16,
    "ayat_arabic": "وَّجَنّٰتٍ اَلْفَافًاۗ",
    "ayat_bahasa": "dan kebun-kebun yang rindang.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078016.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5689,
    "ayat_number": 17,
    "ayat_arabic": "اِنَّ يَوْمَ الْفَصْلِ كَانَ مِيْقَاتًاۙ",
    "ayat_bahasa": "Sungguh, hari keputusan adalah suatu waktu yang telah ditetapkan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078017.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5690,
    "ayat_number": 18,
    "ayat_arabic": "يَّوْمَ يُنْفَخُ فِى الصُّوْرِ فَتَأْتُوْنَ اَفْوَاجًاۙ",
    "ayat_bahasa": "(yaitu) pada hari (ketika) sangkakala ditiup, lalu kamu datang berbondong-bondong,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078018.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5691,
    "ayat_number": 19,
    "ayat_arabic": "وَّفُتِحَتِ السَّمَاۤءُ فَكَانَتْ اَبْوَابًاۙ",
    "ayat_bahasa": "dan langit pun dibukalah, maka terdapatlah beberapa pintu,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078019.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5692,
    "ayat_number": 20,
    "ayat_arabic": "وَّسُيِّرَتِ الْجِبَالُ فَكَانَتْ سَرَابًاۗ",
    "ayat_bahasa": "dan gunung-gunung pun dijalankan sehingga menjadi fatamorgana.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078020.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5693,
    "ayat_number": 21,
    "ayat_arabic": "اِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًاۙ",
    "ayat_bahasa": "Sungguh, (neraka) Jahanam itu (sebagai) tempat mengintai (bagi penjaga yang mengawasi isi neraka), ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078021.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5694,
    "ayat_number": 22,
    "ayat_arabic": "لِّلطّٰغِيْنَ مَاٰبًاۙ",
    "ayat_bahasa": "menjadi tempat kembali bagi orang-orang yang melampaui batas.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078022.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5695,
    "ayat_number": 23,
    "ayat_arabic": "لّٰبِثِيْنَ فِيْهَآ اَحْقَابًاۚ",
    "ayat_bahasa": "Mereka tinggal di sana dalam masa yang lama,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078023.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5696,
    "ayat_number": 24,
    "ayat_arabic": "لَا يَذُوْقُوْنَ فِيْهَا بَرْدًا وَّلَا شَرَابًاۙ",
    "ayat_bahasa": "mereka tidak merasakan kesejukan di dalamnya dan tidak (pula mendapat) minuman,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078024.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5697,
    "ayat_number": 25,
    "ayat_arabic": "اِلَّا حَمِيْمًا وَّغَسَّاقًاۙ",
    "ayat_bahasa": "selain air yang mendidih dan nanah,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078025.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5698,
    "ayat_number": 26,
    "ayat_arabic": "جَزَاۤءً وِّفَاقًاۗ",
    "ayat_bahasa": "sebagai pembalasan yang setimpal.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078026.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5699,
    "ayat_number": 27,
    "ayat_arabic": "اِنَّهُمْ كَانُوْا لَا يَرْجُوْنَ حِسَابًاۙ",
    "ayat_bahasa": "Sesungguhnya dahulu mereka tidak pernah mengharapkan perhitungan.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078027.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5700,
    "ayat_number": 28,
    "ayat_arabic": "وَّكَذَّبُوْا بِاٰيٰتِنَا كِذَّابًاۗ",
    "ayat_bahasa": "Dan mereka benar-benar mendustakan ayat-ayat Kami.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078028.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5701,
    "ayat_number": 29,
    "ayat_arabic": "وَكُلَّ شَيْءٍ اَحْصَيْنٰهُ كِتٰبًاۙ",
    "ayat_bahasa": "Dan segala sesuatu telah Kami catat dalam suatu Kitab (buku catatan amalan manusia). ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078029.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5702,
    "ayat_number": 30,
    "ayat_arabic": "فَذُوْقُوْا فَلَنْ نَّزِيْدَكُمْ اِلَّا عَذَابًا ࣖ",
    "ayat_bahasa": "Maka karena itu rasakanlah! Maka tidak ada yang akan Kami tambahkan kepadamu selain azab.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078030.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 582
  },
  {
    "ayat_id": 5703,
    "ayat_number": 31,
    "ayat_arabic": "اِنَّ لِلْمُتَّقِيْنَ مَفَازًاۙ",
    "ayat_bahasa": "Sungguh, orang-orang yang bertakwa mendapat kemenangan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078031.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5704,
    "ayat_number": 32,
    "ayat_arabic": "حَدَاۤىِٕقَ وَاَعْنَابًاۙ",
    "ayat_bahasa": "(yaitu) kebun-kebun dan buah anggur,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078032.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5705,
    "ayat_number": 33,
    "ayat_arabic": "وَّكَوَاعِبَ اَتْرَابًاۙ",
    "ayat_bahasa": "dan gadis-gadis montok yang sebaya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078033.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5706,
    "ayat_number": 34,
    "ayat_arabic": "وَّكَأْسًا دِهَاقًاۗ",
    "ayat_bahasa": "dan gelas-gelas yang penuh (berisi minuman).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078034.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5707,
    "ayat_number": 35,
    "ayat_arabic": "لَا يَسْمَعُوْنَ فِيْهَا لَغْوًا وَّلَا كِذَّابًا",
    "ayat_bahasa": "Di sana mereka tidak mendengar percakapan yang sia-sia maupun (perkataan) dusta.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078035.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5708,
    "ayat_number": 36,
    "ayat_arabic": "جَزَاۤءً مِّنْ رَّبِّكَ عَطَاۤءً حِسَابًاۙ",
    "ayat_bahasa": "Sebagai balasan dan pemberian yang cukup banyak dari Tuhanmu,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078036.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5709,
    "ayat_number": 37,
    "ayat_arabic": "رَّبِّ السَّمٰوٰتِ وَالْاَرْضِ وَمَا بَيْنَهُمَا الرَّحْمٰنِ لَا يَمْلِكُوْنَ مِنْهُ خِطَابًاۚ ",
    "ayat_bahasa": "Tuhan (yang memelihara) langit dan bumi dan apa yang ada di antara keduanya; Yang Maha Pengasih, mereka tidak mampu berbicara dengan Dia.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078037.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5710,
    "ayat_number": 38,
    "ayat_arabic": "يَوْمَ يَقُوْمُ الرُّوْحُ وَالْمَلٰۤىِٕكَةُ صَفًّاۙ  لَّا يَتَكَلَّمُوْنَ اِلَّا مَنْ اَذِنَ لَهُ الرَّحْمٰنُ وَقَالَ صَوَابًا",
    "ayat_bahasa": "Pada hari, ketika ruh dan para malaikat berdiri bersaf-saf, mereka tidak berkata-kata, kecuali siapa yang telah diberi izin kepadanya oleh Tuhan Yang Maha Pengasih dan dia hanya mengatakan yang benar.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078038.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5711,
    "ayat_number": 39,
    "ayat_arabic": "ذٰلِكَ الْيَوْمُ الْحَقُّۚ فَمَنْ شَاۤءَ اتَّخَذَ اِلٰى رَبِّهٖ مَاٰبًا",
    "ayat_bahasa": "Itulah hari yang pasti terjadi. Maka barang siapa menghendaki, niscaya dia menempuh jalan kembali kepada Tuhannya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078039.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5712,
    "ayat_number": 40,
    "ayat_arabic": "اِنَّآ اَنْذَرْنٰكُمْ عَذَابًا قَرِيْبًا ەۙ يَّوْمَ يَنْظُرُ الْمَرْءُ مَا قَدَّمَتْ يَدَاهُ وَيَقُوْلُ الْكٰفِرُ يٰلَيْتَنِيْ كُنْتُ تُرَابًا ࣖ",
    "ayat_bahasa": "Sesungguhnya Kami telah memperingatkan kepadamu (orang kafir) azab yang dekat, pada hari manusia melihat apa yang telah diperbuat oleh kedua tangannya; dan orang kafir berkata, “Alangkah baiknya seandainya dahulu aku jadi tanah.”",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/078040.mp3",
    "surat_id": 78,
    "juz_id": 30,
    "page_number": 583
  }
]
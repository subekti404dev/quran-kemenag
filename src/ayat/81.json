[
  {
    "ayat_id": 5801,
    "ayat_number": 1,
    "ayat_arabic": "اِذَا الشَّمْسُ كُوِّرَتْۖ",
    "ayat_bahasa": "Apabila matahari digulung,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081001.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5802,
    "ayat_number": 2,
    "ayat_arabic": "وَاِذَا النُّجُوْمُ انْكَدَرَتْۖ",
    "ayat_bahasa": "dan apabila bintang-bintang berjatuhan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081002.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5803,
    "ayat_number": 3,
    "ayat_arabic": "وَاِذَا الْجِبَالُ سُيِّرَتْۖ",
    "ayat_bahasa": "dan apabila gunung-gunung dihancurkan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081003.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5804,
    "ayat_number": 4,
    "ayat_arabic": "وَاِذَا الْعِشَارُ عُطِّلَتْۖ",
    "ayat_bahasa": "dan apabila unta-unta yang bunting ditinggalkan (tidak terurus),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081004.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5805,
    "ayat_number": 5,
    "ayat_arabic": "وَاِذَا الْوُحُوْشُ حُشِرَتْۖ",
    "ayat_bahasa": "dan apabila binatang-binatang liar dikumpulkan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081005.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5806,
    "ayat_number": 6,
    "ayat_arabic": "وَاِذَا الْبِحَارُ سُجِّرَتْۖ",
    "ayat_bahasa": "dan apabila lautan dipanaskan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081006.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5807,
    "ayat_number": 7,
    "ayat_arabic": "وَاِذَا النُّفُوْسُ زُوِّجَتْۖ",
    "ayat_bahasa": "dan apabila roh-roh dipertemukan (dengan tubuh), ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081007.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5808,
    "ayat_number": 8,
    "ayat_arabic": "وَاِذَا الْمَوْءٗدَةُ سُىِٕلَتْۖ ",
    "ayat_bahasa": "dan apabila bayi-bayi perempuan yang dikubur hidup-hidup ditanya, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081008.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5809,
    "ayat_number": 9,
    "ayat_arabic": "بِاَيِّ ذَنْۢبٍ قُتِلَتْۚ",
    "ayat_bahasa": "karena dosa apa dia dibunuh? ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081009.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5810,
    "ayat_number": 10,
    "ayat_arabic": "وَاِذَا الصُّحُفُ نُشِرَتْۖ",
    "ayat_bahasa": "Dan apabila lembaran-lembaran (catatan amal) telah dibuka lebar-lebar, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081010.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5811,
    "ayat_number": 11,
    "ayat_arabic": "وَاِذَا السَّمَاۤءُ كُشِطَتْۖ",
    "ayat_bahasa": "dan apabila langit dilenyapkan, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081011.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5812,
    "ayat_number": 12,
    "ayat_arabic": "وَاِذَا الْجَحِيْمُ سُعِّرَتْۖ",
    "ayat_bahasa": "dan apabila neraka Jahim dinyalakan, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081012.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5813,
    "ayat_number": 13,
    "ayat_arabic": "وَاِذَا الْجَنَّةُ اُزْلِفَتْۖ",
    "ayat_bahasa": "dan apabila surga didekatkan, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081013.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5814,
    "ayat_number": 14,
    "ayat_arabic": "عَلِمَتْ نَفْسٌ مَّآ اَحْضَرَتْۗ",
    "ayat_bahasa": "setiap jiwa akan mengetahui apa yang telah dikerjakannya. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081014.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5815,
    "ayat_number": 15,
    "ayat_arabic": "فَلَآ اُقْسِمُ بِالْخُنَّسِۙ",
    "ayat_bahasa": "Aku bersumpah demi bintang-bintang,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081015.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5816,
    "ayat_number": 16,
    "ayat_arabic": "الْجَوَارِ الْكُنَّسِۙ",
    "ayat_bahasa": "yang beredar dan terbenam, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081016.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5817,
    "ayat_number": 17,
    "ayat_arabic": "وَالَّيْلِ اِذَا عَسْعَسَۙ",
    "ayat_bahasa": "demi malam apabila telah larut, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081017.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5818,
    "ayat_number": 18,
    "ayat_arabic": "وَالصُّبْحِ اِذَا تَنَفَّسَۙ",
    "ayat_bahasa": "dan demi subuh apabila fajar telah menyingsing, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081018.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5819,
    "ayat_number": 19,
    "ayat_arabic": "اِنَّهٗ لَقَوْلُ رَسُوْلٍ كَرِيْمٍۙ",
    "ayat_bahasa": "sesungguhnya (Al-Qur'an) itu benar-benar firman (Allah yang dibawa oleh) utusan yang mulia (Jibril),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081019.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5820,
    "ayat_number": 20,
    "ayat_arabic": "ذِيْ قُوَّةٍ عِنْدَ ذِى الْعَرْشِ مَكِيْنٍۙ",
    "ayat_bahasa": "yang memiliki kekuatan, memiliki kedudukan tinggi di sisi (Allah) yang memiliki ‘Arsy,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081020.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5821,
    "ayat_number": 21,
    "ayat_arabic": "مُّطَاعٍ ثَمَّ اَمِيْنٍۗ",
    "ayat_bahasa": "yang di sana (di alam malaikat) ditaati dan dipercaya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081021.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5822,
    "ayat_number": 22,
    "ayat_arabic": "وَمَا صَاحِبُكُمْ بِمَجْنُوْنٍۚ",
    "ayat_bahasa": "Dan temanmu (Muhammad) itu bukanlah orang gila. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081022.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5823,
    "ayat_number": 23,
    "ayat_arabic": "وَلَقَدْ رَاٰهُ بِالْاُفُقِ الْمُبِيْنِۚ",
    "ayat_bahasa": "Dan sungguh, dia (Muhammad) telah melihatnya (Jibril) di ufuk yang terang. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081023.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5824,
    "ayat_number": 24,
    "ayat_arabic": "وَمَا هُوَ عَلَى الْغَيْبِ بِضَنِيْنٍۚ",
    "ayat_bahasa": "Dan dia (Muhammad) bukanlah seorang yang kikir (enggan) untuk menerangkan yang gaib.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081024.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5825,
    "ayat_number": 25,
    "ayat_arabic": "وَمَا هُوَ بِقَوْلِ شَيْطٰنٍ رَّجِيْمٍۚ",
    "ayat_bahasa": "Dan (Al-Qur'an) itu bukanlah perkataan setan yang terkutuk,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081025.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5826,
    "ayat_number": 26,
    "ayat_arabic": "فَاَيْنَ تَذْهَبُوْنَۗ",
    "ayat_bahasa": "maka ke manakah kamu akan pergi?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081026.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5827,
    "ayat_number": 27,
    "ayat_arabic": "اِنْ هُوَ اِلَّا ذِكْرٌ لِّلْعٰلَمِيْنَۙ",
    "ayat_bahasa": "(Al-Qur'an) itu tidak lain adalah peringatan bagi seluruh alam, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081027.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5828,
    "ayat_number": 28,
    "ayat_arabic": "لِمَنْ شَاۤءَ مِنْكُمْ اَنْ يَّسْتَقِيْمَۗ",
    "ayat_bahasa": "(yaitu) bagi siapa di antara kamu yang menghendaki menempuh jalan yang lurus. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081028.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  },
  {
    "ayat_id": 5829,
    "ayat_number": 29,
    "ayat_arabic": "وَمَا تَشَاۤءُوْنَ اِلَّآ اَنْ يَّشَاۤءَ اللّٰهُ رَبُّ الْعٰلَمِيْنَ ࣖ",
    "ayat_bahasa": "Dan kamu tidak dapat menghendaki (menempuh jalan itu) kecuali apabila dikehendaki Allah, Tuhan seluruh alam.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/081029.mp3",
    "surat_id": 81,
    "juz_id": 30,
    "page_number": 586
  }
]
[
  {
    "ayat_id": 5759,
    "ayat_number": 1,
    "ayat_arabic": "عَبَسَ وَتَوَلّٰىٓۙ",
    "ayat_bahasa": "Dia (Muhammad) berwajah masam dan berpaling,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080001.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5760,
    "ayat_number": 2,
    "ayat_arabic": "اَنْ جَاۤءَهُ الْاَعْمٰىۗ",
    "ayat_bahasa": "karena seorang buta telah datang kepadanya (Abdullah bin Ummi Maktum).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080002.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5761,
    "ayat_number": 3,
    "ayat_arabic": "وَمَا يُدْرِيْكَ لَعَلَّهٗ يَزَّكّٰىٓۙ",
    "ayat_bahasa": "Dan tahukah engkau (Muhammad) barangkali dia ingin menyucikan dirinya (dari dosa),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080003.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5762,
    "ayat_number": 4,
    "ayat_arabic": "اَوْ يَذَّكَّرُ فَتَنْفَعَهُ الذِّكْرٰىۗ",
    "ayat_bahasa": "atau dia (ingin) mendapatkan pengajaran, yang memberi manfaat kepadanya?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080004.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5763,
    "ayat_number": 5,
    "ayat_arabic": "اَمَّا مَنِ اسْتَغْنٰىۙ",
    "ayat_bahasa": "Adapun orang yang merasa dirinya serba cukup (pembesar-pembesar Quraisy),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080005.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5764,
    "ayat_number": 6,
    "ayat_arabic": "فَاَنْتَ لَهٗ تَصَدّٰىۗ",
    "ayat_bahasa": "maka engkau (Muhammad) memberi perhatian kepadanya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080006.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5765,
    "ayat_number": 7,
    "ayat_arabic": "وَمَا عَلَيْكَ اَلَّا يَزَّكّٰىۗ",
    "ayat_bahasa": "padahal tidak ada (cela) atasmu kalau dia tidak menyucikan diri (beriman).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080007.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5766,
    "ayat_number": 8,
    "ayat_arabic": "وَاَمَّا مَنْ جَاۤءَكَ يَسْعٰىۙ",
    "ayat_bahasa": "Dan adapun orang yang datang kepadamu dengan bersegera (untuk mendapatkan pengajaran),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080008.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5767,
    "ayat_number": 9,
    "ayat_arabic": "وَهُوَ يَخْشٰىۙ",
    "ayat_bahasa": "sedang dia takut (kepada Allah),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080009.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5768,
    "ayat_number": 10,
    "ayat_arabic": "فَاَنْتَ عَنْهُ تَلَهّٰىۚ",
    "ayat_bahasa": "engkau (Muhammad) malah mengabaikannya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080010.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5769,
    "ayat_number": 11,
    "ayat_arabic": "كَلَّآ اِنَّهَا تَذْكِرَةٌ ۚ",
    "ayat_bahasa": "Sekali-kali jangan (begitu)! Sungguh, (ajaran-ajaran Allah) itu suatu peringatan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080011.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5770,
    "ayat_number": 12,
    "ayat_arabic": "فَمَنْ شَاۤءَ ذَكَرَهٗ ۘ",
    "ayat_bahasa": "maka barangsiapa menghendaki, tentulah dia akan memperhatikannya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080012.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5771,
    "ayat_number": 13,
    "ayat_arabic": "فِيْ صُحُفٍ مُّكَرَّمَةٍۙ",
    "ayat_bahasa": "di dalam kitab-kitab yang dimuliakan (di sisi Allah),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080013.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5772,
    "ayat_number": 14,
    "ayat_arabic": "مَّرْفُوْعَةٍ مُّطَهَّرَةٍ ۢ ۙ",
    "ayat_bahasa": "yang ditinggikan (dan) disucikan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080014.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5773,
    "ayat_number": 15,
    "ayat_arabic": "بِاَيْدِيْ سَفَرَةٍۙ",
    "ayat_bahasa": "di tangan para utusan (malaikat),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080015.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5774,
    "ayat_number": 16,
    "ayat_arabic": "كِرَامٍۢ بَرَرَةٍۗ",
    "ayat_bahasa": "yang mulia lagi berbakti.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080016.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5775,
    "ayat_number": 17,
    "ayat_arabic": "قُتِلَ الْاِنْسَانُ مَآ اَكْفَرَهٗۗ",
    "ayat_bahasa": "Celakalah manusia! Alangkah kufurnya dia!",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080017.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5776,
    "ayat_number": 18,
    "ayat_arabic": "مِنْ اَيِّ شَيْءٍ خَلَقَهٗۗ",
    "ayat_bahasa": "Dari apakah Dia (Allah) menciptakannya?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080018.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5777,
    "ayat_number": 19,
    "ayat_arabic": "مِنْ نُّطْفَةٍۗ خَلَقَهٗ فَقَدَّرَهٗۗ",
    "ayat_bahasa": "Dari setetes mani, Dia menciptakannya lalu menentukannya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080019.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5778,
    "ayat_number": 20,
    "ayat_arabic": "ثُمَّ السَّبِيْلَ يَسَّرَهٗۙ",
    "ayat_bahasa": "Kemudian jalannya Dia mudahkan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080020.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5779,
    "ayat_number": 21,
    "ayat_arabic": "ثُمَّ اَمَاتَهٗ فَاَقْبَرَهٗۙ",
    "ayat_bahasa": "kemudian Dia mematikannya lalu menguburkannya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080021.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5780,
    "ayat_number": 22,
    "ayat_arabic": "ثُمَّ اِذَا شَاۤءَ اَنْشَرَهٗۗ",
    "ayat_bahasa": "kemudian jika Dia menghendaki, Dia membangkitkannya kembali.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080022.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5781,
    "ayat_number": 23,
    "ayat_arabic": "كَلَّا لَمَّا يَقْضِ مَآ اَمَرَهٗۗ",
    "ayat_bahasa": "Sekali-kali jangan (begitu)! Dia (manusia) itu belum melaksanakan apa yang Dia (Allah) perintahkan kepadanya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080023.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5782,
    "ayat_number": 24,
    "ayat_arabic": "فَلْيَنْظُرِ الْاِنْسَانُ اِلٰى طَعَامِهٖٓ ۙ ",
    "ayat_bahasa": "Maka hendaklah manusia itu memperhatikan makanannya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080024.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5783,
    "ayat_number": 25,
    "ayat_arabic": "اَنَّا صَبَبْنَا الْمَاۤءَ صَبًّاۙ",
    "ayat_bahasa": "Kamilah yang telah mencurahkan air melimpah (dari langit),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080025.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5784,
    "ayat_number": 26,
    "ayat_arabic": "ثُمَّ شَقَقْنَا الْاَرْضَ شَقًّاۙ",
    "ayat_bahasa": "kemudian Kami belah bumi dengan sebaik-baiknya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080026.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5785,
    "ayat_number": 27,
    "ayat_arabic": "فَاَنْۢبَتْنَا فِيْهَا حَبًّاۙ",
    "ayat_bahasa": "lalu di sana Kami tumbuhkan biji-bijian,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080027.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5786,
    "ayat_number": 28,
    "ayat_arabic": "وَّعِنَبًا وَّقَضْبًاۙ",
    "ayat_bahasa": "dan anggur dan sayur-sayuran,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080028.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5787,
    "ayat_number": 29,
    "ayat_arabic": "وَّزَيْتُوْنًا وَّنَخْلًاۙ",
    "ayat_bahasa": "dan zaitun dan pohon kurma,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080029.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5788,
    "ayat_number": 30,
    "ayat_arabic": "وَّحَدَاۤئِقَ غُلْبًا",
    "ayat_bahasa": "dan kebun-kebun (yang) rindang,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080030.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5789,
    "ayat_number": 31,
    "ayat_arabic": "وَفَاكِهَةً وَّاَبًّا",
    "ayat_bahasa": "dan buah-buahan serta rerumputan.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080031.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5790,
    "ayat_number": 32,
    "ayat_arabic": "مَتَاعًا لَّكُمْ وَلِاَنْعَامِكُمْۗ",
    "ayat_bahasa": "(Semua itu) untuk kesenanganmu dan untuk hewan-hewan ternakmu.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080032.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5791,
    "ayat_number": 33,
    "ayat_arabic": "فَاِذَا جَاۤءَتِ الصَّاۤخَّةُ ۖ",
    "ayat_bahasa": "Maka apabila datang suara yang memekakkan (tiupan sangkakala yang kedua),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080033.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5792,
    "ayat_number": 34,
    "ayat_arabic": "يَوْمَ يَفِرُّ الْمَرْءُ مِنْ اَخِيْهِۙ",
    "ayat_bahasa": "pada hari itu manusia lari dari saudaranya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080034.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5793,
    "ayat_number": 35,
    "ayat_arabic": "وَاُمِّهٖ وَاَبِيْهِۙ",
    "ayat_bahasa": "dan dari ibu dan bapaknya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080035.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5794,
    "ayat_number": 36,
    "ayat_arabic": "وَصَاحِبَتِهٖ وَبَنِيْهِۗ",
    "ayat_bahasa": "dan dari istri dan anak-anaknya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080036.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5795,
    "ayat_number": 37,
    "ayat_arabic": "لِكُلِّ امْرِئٍ مِّنْهُمْ يَوْمَىِٕذٍ شَأْنٌ يُّغْنِيْهِۗ",
    "ayat_bahasa": "Setiap orang dari mereka pada hari itu mempunyai urusan yang menyibukkannya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080037.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5796,
    "ayat_number": 38,
    "ayat_arabic": "وُجُوْهٌ يَّوْمَىِٕذٍ مُّسْفِرَةٌۙ",
    "ayat_bahasa": "Pada hari itu ada wajah-wajah yang berseri-seri,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080038.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5797,
    "ayat_number": 39,
    "ayat_arabic": "ضَاحِكَةٌ مُّسْتَبْشِرَةٌ ۚ",
    "ayat_bahasa": "tertawa dan gembira ria,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080039.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5798,
    "ayat_number": 40,
    "ayat_arabic": "وَوُجُوْهٌ يَّوْمَىِٕذٍ عَلَيْهَا غَبَرَةٌۙ",
    "ayat_bahasa": "dan pada hari itu ada (pula) wajah-wajah yang tertutup debu (suram),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080040.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5799,
    "ayat_number": 41,
    "ayat_arabic": "تَرْهَقُهَا قَتَرَةٌ ۗ",
    "ayat_bahasa": "tertutup oleh kegelapan (ditimpa kehinaan dan kesusahan).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080041.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  },
  {
    "ayat_id": 5800,
    "ayat_number": 42,
    "ayat_arabic": "اُولٰۤىِٕكَ هُمُ الْكَفَرَةُ الْفَجَرَةُ ࣖ",
    "ayat_bahasa": "Mereka itulah orang-orang kafir yang durhaka.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/080042.mp3",
    "surat_id": 80,
    "juz_id": 30,
    "page_number": 585
  }
]
    "surat_id": 84,
    "juz_id": 30,
    "page_number": 589
  },
  {
    "ayat_id": 5895,
    "ayat_number": 11,
    "ayat_arabic": "فَسَوْفَ يَدْعُوْ ثُبُوْرًاۙ ",
    "ayat_bahasa": "maka dia akan berteriak, “Celakalah aku!” ",

    "ayat_bahasa": "dan apabila bumi diratakan, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/084003.mp3",
    "surat_id": 84,
    "juz_id": 30,
    "page_number": 589
  },
  {
    "ayat_id": 5888,

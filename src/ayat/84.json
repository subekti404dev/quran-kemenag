  },
  {
    "ayat_id": 5890,
    "ayat_number": 6,
    "ayat_arabic": "يٰٓاَيُّهَا الْاِنْسَانُ اِنَّكَ كَادِحٌ اِلٰى رَبِّكَ كَدْحًا فَمُلٰقِيْهِۚ ",
    "ayat_bahasa": "Wahai manusia! Sesungguhnya kamu telah bekerja keras menuju Tuhanmu, maka kamu akan menemui-Nya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/084006.mp3",
    "surat_id": 84,
    "juz_id": 30,
    "page_number": 589
  },
  {
    "ayat_id": 5891,
    "ayat_number": 7,
    "ayat_arabic": "فَاَمَّا مَنْ اُوْتِيَ كِتٰبَهٗ بِيَمِيْنِهٖۙ ",
    "ayat_bahasa": "Maka adapun orang yang catatannya diberikan dari sebelah kanannya, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/084007.mp3",
    "surat_id": 84,
    "juz_id": 30,
    "page_number": 589
  },
  {
    "ayat_id": 5892,
    "ayat_number": 8,
    "ayat_arabic": "فَسَوْفَ يُحَاسَبُ حِسَابًا يَّسِيْرًاۙ ",
    "ayat_bahasa": "maka dia akan diperiksa dengan pemeriksaan yang mudah, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/084008.mp3",
    "surat_id": 84,
    "juz_id": 30,
    "page_number": 589
  },
  {
    "ayat_id": 5893,
    "ayat_number": 9,
    "ayat_arabic": "وَّيَنْقَلِبُ اِلٰٓى اَهْلِهٖ مَسْرُوْرًاۗ ",
    "ayat_bahasa": "dan dia akan kembali kepada keluarganya (yang sama-sama beriman) dengan gembira.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/084009.mp3",
    "surat_id": 84,
    "juz_id": 30,
    "page_number": 589
  },
  {
    "ayat_id": 5894,
    "ayat_number": 10,
    "ayat_arabic": "وَاَمَّا مَنْ اُوْتِيَ كِتٰبَهٗ وَرَاۤءَ ظَهْرِهٖۙ ",
    "ayat_bahasa": "Dan adapun orang yang catatannya diberikan dari sebelah belakang, ",

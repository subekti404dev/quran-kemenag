[
  {
    "ayat_id": 5994,
    "ayat_number": 1,
    "ayat_arabic": "وَالْفَجْرِۙ",
    "ayat_bahasa": "Demi fajar,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089001.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 5995,
    "ayat_number": 2,
    "ayat_arabic": "وَلَيَالٍ عَشْرٍۙ",
    "ayat_bahasa": "demi malam yang sepuluh,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089002.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 5996,
    "ayat_number": 3,
    "ayat_arabic": "وَّالشَّفْعِ وَالْوَتْرِۙ",
    "ayat_bahasa": "demi yang genap dan yang ganjil,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089003.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 5997,
    "ayat_number": 4,
    "ayat_arabic": "وَالَّيْلِ اِذَا يَسْرِۚ",
    "ayat_bahasa": "demi malam apabila berlalu.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089004.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 5998,
    "ayat_number": 5,
    "ayat_arabic": "هَلْ فِيْ ذٰلِكَ قَسَمٌ لِّذِيْ حِجْرٍۗ",
    "ayat_bahasa": "Adakah pada yang demikian itu terdapat sumpah (yang dapat diterima) bagi orang-orang yang berakal?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089005.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 5999,
    "ayat_number": 6,
    "ayat_arabic": "اَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍۖ",
    "ayat_bahasa": "Tidakkah engkau (Muhammad) memperhatikan bagaimana Tuhanmu berbuat terhadap (kaum) ‘Ad?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089006.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6000,
    "ayat_number": 7,
    "ayat_arabic": "اِرَمَ ذَاتِ الْعِمَادِۖ",
    "ayat_bahasa": "(yaitu) penduduk Iram (ibukota kaum ‘Ad) yang mempunyai bangunan-bangunan yang tinggi,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089007.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6001,
    "ayat_number": 8,
    "ayat_arabic": "الَّتِيْ لَمْ يُخْلَقْ مِثْلُهَا فِى الْبِلَادِۖ",
    "ayat_bahasa": "yang belum pernah dibangun (suatu kota) seperti itu di negeri-negeri lain,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089008.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6002,
    "ayat_number": 9,
    "ayat_arabic": "وَثَمُوْدَ الَّذِيْنَ جَابُوا الصَّخْرَ بِالْوَادِۖ",
    "ayat_bahasa": "dan (terhadap) kaum samud yang memotong batu-batu besar di lembah,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089009.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6003,
    "ayat_number": 10,
    "ayat_arabic": "وَفِرْعَوْنَ ذِى الْاَوْتَادِۖ",
    "ayat_bahasa": "dan (terhadap) Fir‘aun yang mempunyai pasak-pasak (bangunan yang besar),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089010.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6004,
    "ayat_number": 11,
    "ayat_arabic": "الَّذِيْنَ طَغَوْا فِى الْبِلَادِۖ",
    "ayat_bahasa": "yang berbuat sewenang-wenang dalam negeri,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089011.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6005,
    "ayat_number": 12,
    "ayat_arabic": "فَاَكْثَرُوْا فِيْهَا الْفَسَادَۖ",
    "ayat_bahasa": "lalu mereka banyak berbuat kerusakan dalam negeri itu,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089012.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6006,
    "ayat_number": 13,
    "ayat_arabic": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍۖ",
    "ayat_bahasa": "karena itu Tuhanmu menimpakan cemeti azab kepada mereka,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089013.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6007,
    "ayat_number": 14,
    "ayat_arabic": "اِنَّ رَبَّكَ لَبِالْمِرْصَادِۗ",
    "ayat_bahasa": "sungguh, Tuhanmu benar-benar mengawasi.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089014.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6008,
    "ayat_number": 15,
    "ayat_arabic": "فَاَمَّا الْاِنْسَانُ اِذَا مَا ابْتَلٰىهُ رَبُّهٗ فَاَكْرَمَهٗ وَنَعَّمَهٗۙ فَيَقُوْلُ رَبِّيْٓ اَكْرَمَنِۗ",
    "ayat_bahasa": "Maka adapun manusia, apabila Tuhan mengujinya lalu memuliakannya dan memberinya kesenangan, maka dia berkata, “Tuhanku telah memuliakanku.”",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089015.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6009,
    "ayat_number": 16,
    "ayat_arabic": "وَاَمَّآ اِذَا مَا ابْتَلٰىهُ فَقَدَرَ عَلَيْهِ رِزْقَهٗ ەۙ فَيَقُوْلُ رَبِّيْٓ اَهَانَنِۚ",
    "ayat_bahasa": "Namun apabila Tuhan mengujinya lalu membatasi rezekinya, maka dia berkata, “Tuhanku telah menghinaku.”",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089016.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6010,
    "ayat_number": 17,
    "ayat_arabic": "كَلَّا بَلْ لَّا تُكْرِمُوْنَ الْيَتِيْمَۙ",
    "ayat_bahasa": "Sekali-kali tidak! Bahkan kamu tidak memuliakan anak yatim,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089017.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6011,
    "ayat_number": 18,
    "ayat_arabic": "وَلَا تَحٰۤضُّوْنَ عَلٰى طَعَامِ الْمِسْكِيْنِۙ",
    "ayat_bahasa": "dan kamu tidak saling mengajak memberi makan orang miskin,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089018.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6012,
    "ayat_number": 19,
    "ayat_arabic": "وَتَأْكُلُوْنَ التُّرَاثَ اَكْلًا لَّمًّاۙ",
    "ayat_bahasa": "sedangkan kamu memakan harta warisan dengan cara mencampurbaurkan (yang halal dan yang haram),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089019.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6013,
    "ayat_number": 20,
    "ayat_arabic": "وَّتُحِبُّوْنَ الْمَالَ حُبًّا جَمًّاۗ",
    "ayat_bahasa": "dan kamu mencintai harta dengan kecintaan yang berlebihan.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089020.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6014,
    "ayat_number": 21,
    "ayat_arabic": "كَلَّآ اِذَا دُكَّتِ الْاَرْضُ دَكًّا دَكًّاۙ",
    "ayat_bahasa": "Sekali-kali tidak! Apabila bumi diguncangkan berturut-turut (berbenturan),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089021.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6015,
    "ayat_number": 22,
    "ayat_arabic": "وَّجَآءَ رَبُّكَ وَالْمَلَكُ صَفًّا صَفًّاۚ",
    "ayat_bahasa": "dan datanglah Tuhanmu; dan malaikat berbaris-baris,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089022.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6016,
    "ayat_number": 23,
    "ayat_arabic": "وَجِايْۤءَ يَوْمَىِٕذٍۢ بِجَهَنَّمَۙ يَوْمَىِٕذٍ يَّتَذَكَّرُ الْاِنْسَانُ وَاَنّٰى لَهُ الذِّكْرٰىۗ",
    "ayat_bahasa": "dan pada hari itu diperlihatkan neraka Jahanam; pada hari itu sadarlah manusia, tetapi tidak berguna lagi baginya kesadaran itu.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089023.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 593
  },
  {
    "ayat_id": 6017,
    "ayat_number": 24,
    "ayat_arabic": "يَقُوْلُ يٰلَيْتَنِيْ قَدَّمْتُ لِحَيَاتِيْۚ",
    "ayat_bahasa": "Dia berkata, “Alangkah baiknya sekiranya dahulu aku mengerjakan (kebajikan) untuk hidupku ini.”",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089024.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 594
  },
  {
    "ayat_id": 6018,
    "ayat_number": 25,
    "ayat_arabic": "فَيَوْمَىِٕذٍ لَّا يُعَذِّبُ عَذَابَهٗٓ اَحَدٌ ۙ",
    "ayat_bahasa": "Maka pada hari itu tidak ada seorang pun yang mengazab seperti azab-Nya (yang adil),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089025.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 594
  },
  {
    "ayat_id": 6019,
    "ayat_number": 26,
    "ayat_arabic": "وَّلَا يُوْثِقُ وَثَاقَهٗٓ اَحَدٌ ۗ",
    "ayat_bahasa": "dan tidak ada seorang pun yang mengikat seperti ikatan-Nya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089026.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 594
  },
  {
    "ayat_id": 6020,
    "ayat_number": 27,
    "ayat_arabic": "يٰٓاَيَّتُهَا النَّفْسُ الْمُطْمَىِٕنَّةُۙ",
    "ayat_bahasa": "Wahai jiwa yang tenang!",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089027.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 594
  },
  {
    "ayat_id": 6021,
    "ayat_number": 28,
    "ayat_arabic": "ارْجِعِيْٓ اِلٰى رَبِّكِ رَاضِيَةً مَّرْضِيَّةً ۚ",
    "ayat_bahasa": "Kembalilah kepada Tuhanmu dengan hati yang rida dan diridai-Nya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089028.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 594
  },
  {
    "ayat_id": 6022,
    "ayat_number": 29,
    "ayat_arabic": "فَادْخُلِيْ فِيْ عِبٰدِيْۙ",
    "ayat_bahasa": "Maka masuklah ke dalam golongan hamba-hamba-Ku,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089029.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 594
  },
  {
    "ayat_id": 6023,
    "ayat_number": 30,
    "ayat_arabic": "وَادْخُلِيْ جَنَّتِيْ ࣖࣖ",
    "ayat_bahasa": "dan masuklah ke dalam surga-Ku.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/089030.mp3",
    "surat_id": 89,
    "juz_id": 30,
    "page_number": 594
  }
]
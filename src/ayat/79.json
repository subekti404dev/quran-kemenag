[
  {
    "ayat_id": 5713,
    "ayat_number": 1,
    "ayat_arabic": "وَالنّٰزِعٰتِ غَرْقًاۙ",
    "ayat_bahasa": "Demi (malaikat) yang mencabut (nyawa) dengan keras.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079001.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5714,
    "ayat_number": 2,
    "ayat_arabic": "وَّالنّٰشِطٰتِ نَشْطًاۙ",
    "ayat_bahasa": "Demi (malaikat) yang mencabut (nyawa) dengan lemah lembut.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079002.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5715,
    "ayat_number": 3,
    "ayat_arabic": "وَّالسّٰبِحٰتِ سَبْحًاۙ",
    "ayat_bahasa": "Demi (malaikat) yang turun dari langit dengan cepat,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079003.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5716,
    "ayat_number": 4,
    "ayat_arabic": "فَالسّٰبِقٰتِ سَبْقًاۙ",
    "ayat_bahasa": "dan (malaikat) yang mendahului dengan kencang,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079004.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5717,
    "ayat_number": 5,
    "ayat_arabic": "فَالْمُدَبِّرٰتِ اَمْرًاۘ",
    "ayat_bahasa": "dan (malaikat) yang mengatur urusan (dunia).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079005.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5718,
    "ayat_number": 6,
    "ayat_arabic": "يَوْمَ تَرْجُفُ الرَّاجِفَةُۙ",
    "ayat_bahasa": "(Sungguh, kamu akan dibangkitkan) pada hari ketika tiupan pertama mengguncangkan alam,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079006.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5719,
    "ayat_number": 7,
    "ayat_arabic": "تَتْبَعُهَا الرَّادِفَةُ ۗ",
    "ayat_bahasa": "(tiupan pertama) itu diiringi oleh tiupan kedua.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079007.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5720,
    "ayat_number": 8,
    "ayat_arabic": "قُلُوْبٌ يَّوْمَىِٕذٍ وَّاجِفَةٌۙ",
    "ayat_bahasa": "Hati manusia pada waktu itu merasa sangat takut,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079008.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5721,
    "ayat_number": 9,
    "ayat_arabic": "اَبْصَارُهَا خَاشِعَةٌ  ۘ",
    "ayat_bahasa": "pandangannya tunduk.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079009.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5722,
    "ayat_number": 10,
    "ayat_arabic": "يَقُوْلُوْنَ ءَاِنَّا لَمَرْدُوْدُوْنَ فِى الْحَافِرَةِۗ",
    "ayat_bahasa": "(Orang-orang kafir) berkata, “Apakah kita benar-benar akan dikembalikan kepada kehidupan yang semula?   ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079010.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5723,
    "ayat_number": 11,
    "ayat_arabic": "ءَاِذَا كُنَّا عِظَامًا نَّخِرَةً ۗ",
    "ayat_bahasa": "Apakah (akan dibangkitkan juga) apabila kita telah menjadi tulang belulang yang hancur?”",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079011.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5724,
    "ayat_number": 12,
    "ayat_arabic": "قَالُوْا تِلْكَ اِذًا كَرَّةٌ خَاسِرَةٌ  ۘ",
    "ayat_bahasa": "Mereka berkata, “Kalau demikian, itu adalah suatu pengembalian yang merugikan.”",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079012.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5725,
    "ayat_number": 13,
    "ayat_arabic": "فَاِنَّمَا هِيَ زَجْرَةٌ وَّاحِدَةٌۙ",
    "ayat_bahasa": "Maka pengembalian itu hanyalah dengan sekali tiupan saja.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079013.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5726,
    "ayat_number": 14,
    "ayat_arabic": "فَاِذَا هُمْ بِالسَّاهِرَةِۗ",
    "ayat_bahasa": "Maka seketika itu mereka hidup kembali di bumi (yang baru).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079014.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5727,
    "ayat_number": 15,
    "ayat_arabic": "هَلْ اَتٰىكَ حَدِيْثُ مُوْسٰىۘ",
    "ayat_bahasa": "Sudahkah sampai kepadamu (Muhammad) kisah Musa?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079015.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 583
  },
  {
    "ayat_id": 5728,
    "ayat_number": 16,
    "ayat_arabic": "اِذْ نَادٰىهُ رَبُّهٗ بِالْوَادِ الْمُقَدَّسِ طُوًىۚ",
    "ayat_bahasa": "Ketika Tuhan memanggilnya (Musa) di lembah suci yaitu Lembah Tuwa;",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079016.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5729,
    "ayat_number": 17,
    "ayat_arabic": "اِذْهَبْ اِلٰى فِرْعَوْنَ اِنَّهٗ طَغٰىۖ",
    "ayat_bahasa": "pergilah engkau kepada Fir‘aun! Sesungguhnya dia telah melampaui batas,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079017.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5730,
    "ayat_number": 18,
    "ayat_arabic": "فَقُلْ هَلْ لَّكَ اِلٰٓى اَنْ تَزَكّٰىۙ",
    "ayat_bahasa": "Maka katakanlah (kepada Fir‘aun), “Adakah keinginanmu untuk membersihkan diri (dari kesesatan),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079018.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5731,
    "ayat_number": 19,
    "ayat_arabic": "وَاَهْدِيَكَ اِلٰى رَبِّكَ فَتَخْشٰىۚ",
    "ayat_bahasa": "dan engkau akan kupimpin ke jalan Tuhanmu agar engkau takut kepada-Nya?”",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079019.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5732,
    "ayat_number": 20,
    "ayat_arabic": "فَاَرٰىهُ الْاٰيَةَ الْكُبْرٰىۖ",
    "ayat_bahasa": "Lalu (Musa) memperlihatkan kepadanya mukjizat yang besar.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079020.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5733,
    "ayat_number": 21,
    "ayat_arabic": "فَكَذَّبَ وَعَصٰىۖ",
    "ayat_bahasa": "Tetapi dia (Fir‘aun) mendustakan dan mendurhakai.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079021.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5734,
    "ayat_number": 22,
    "ayat_arabic": "ثُمَّ اَدْبَرَ يَسْعٰىۖ",
    "ayat_bahasa": "Kemudian dia berpaling seraya berusaha menantang (Musa).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079022.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5735,
    "ayat_number": 23,
    "ayat_arabic": "فَحَشَرَ فَنَادٰىۖ",
    "ayat_bahasa": "Kemudian dia mengumpulkan (pembesar-pembesarnya) lalu berseru (memanggil kaumnya).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079023.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5736,
    "ayat_number": 24,
    "ayat_arabic": "فَقَالَ اَنَا۠ رَبُّكُمُ الْاَعْلٰىۖ",
    "ayat_bahasa": "(Seraya) berkata, “Akulah tuhanmu yang paling tinggi.”",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079024.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5737,
    "ayat_number": 25,
    "ayat_arabic": "فَاَخَذَهُ اللّٰهُ نَكَالَ الْاٰخِرَةِ وَالْاُوْلٰىۗ",
    "ayat_bahasa": "Maka Allah menghukumnya dengan azab di akhirat dan siksaan di dunia.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079025.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5738,
    "ayat_number": 26,
    "ayat_arabic": "اِنَّ فِيْ ذٰلِكَ لَعِبْرَةً لِّمَنْ يَّخْشٰى ۗ ࣖ",
    "ayat_bahasa": "Sungguh, pada yang demikian itu terdapat pelajaran bagi orang yang takut (kepada Allah).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079026.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5739,
    "ayat_number": 27,
    "ayat_arabic": "ءَاَنْتُمْ اَشَدُّ خَلْقًا اَمِ السَّمَاۤءُ ۚ بَنٰىهَاۗ",
    "ayat_bahasa": "Apakah penciptaan kamu yang lebih hebat ataukah langit yang telah dibangun-Nya?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079027.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5740,
    "ayat_number": 28,
    "ayat_arabic": "رَفَعَ سَمْكَهَا فَسَوّٰىهَاۙ",
    "ayat_bahasa": "Dia telah meninggikan bangunannya lalu menyempurnakannya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079028.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5741,
    "ayat_number": 29,
    "ayat_arabic": "وَاَغْطَشَ لَيْلَهَا وَاَخْرَجَ ضُحٰىهَاۖ",
    "ayat_bahasa": "dan Dia menjadikan malamnya (gelap gulita), dan menjadikan siangnya (terang benderang).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079029.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5742,
    "ayat_number": 30,
    "ayat_arabic": "وَالْاَرْضَ بَعْدَ ذٰلِكَ دَحٰىهَاۗ",
    "ayat_bahasa": "Dan setelah itu bumi Dia hamparkan. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079030.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5743,
    "ayat_number": 31,
    "ayat_arabic": "اَخْرَجَ مِنْهَا مَاۤءَهَا وَمَرْعٰىهَاۖ",
    "ayat_bahasa": "Darinya Dia pancarkan mata air, dan (ditumbuhkan) tumbuh-tumbuhannya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079031.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5744,
    "ayat_number": 32,
    "ayat_arabic": "وَالْجِبَالَ اَرْسٰىهَاۙ",
    "ayat_bahasa": "Dan gunung-gunung Dia pancangkan dengan teguh.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079032.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5745,
    "ayat_number": 33,
    "ayat_arabic": "مَتَاعًا لَّكُمْ وَلِاَنْعَامِكُمْۗ",
    "ayat_bahasa": "(Semua itu) untuk kesenanganmu dan untuk hewan-hewan ternakmu.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079033.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5746,
    "ayat_number": 34,
    "ayat_arabic": "فَاِذَا جَاۤءَتِ الطَّاۤمَّةُ الْكُبْرٰىۖ",
    "ayat_bahasa": "Maka apabila malapetaka besar (hari Kiamat) telah datang,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079034.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5747,
    "ayat_number": 35,
    "ayat_arabic": "يَوْمَ يَتَذَكَّرُ الْاِنْسَانُ مَا سَعٰىۙ",
    "ayat_bahasa": "yaitu pada hari (ketika) manusia teringat akan apa yang telah dikerjakannya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079035.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5748,
    "ayat_number": 36,
    "ayat_arabic": "وَبُرِّزَتِ الْجَحِيْمُ لِمَنْ يَّرٰى",
    "ayat_bahasa": "dan neraka diperlihatkan dengan jelas kepada setiap orang yang melihat.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079036.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5749,
    "ayat_number": 37,
    "ayat_arabic": "فَاَمَّا مَنْ طَغٰىۖ",
    "ayat_bahasa": "Maka adapun orang yang melampaui batas,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079037.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5750,
    "ayat_number": 38,
    "ayat_arabic": "وَاٰثَرَ الْحَيٰوةَ الدُّنْيَاۙ",
    "ayat_bahasa": "dan lebih mengutamakan kehidupan dunia,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079038.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5751,
    "ayat_number": 39,
    "ayat_arabic": "فَاِنَّ الْجَحِيْمَ هِيَ الْمَأْوٰىۗ",
    "ayat_bahasa": "maka sungguh, nerakalah tempat tinggalnya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079039.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5752,
    "ayat_number": 40,
    "ayat_arabic": "وَاَمَّا مَنْ خَافَ مَقَامَ رَبِّهٖ وَنَهَى النَّفْسَ عَنِ الْهَوٰىۙ",
    "ayat_bahasa": "Dan adapun orang-orang yang takut kepada kebesaran Tuhannya dan menahan diri dari (keinginan) hawa nafsunya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079040.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5753,
    "ayat_number": 41,
    "ayat_arabic": "فَاِنَّ الْجَنَّةَ هِيَ الْمَأْوٰىۗ",
    "ayat_bahasa": "maka sungguh, surgalah tempat tinggal(nya).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079041.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5754,
    "ayat_number": 42,
    "ayat_arabic": "يَسْـَٔلُوْنَكَ عَنِ السَّاعَةِ اَيَّانَ مُرْسٰىهَاۗ",
    "ayat_bahasa": "Mereka (orang-orang kafir) bertanya kepadamu (Muhammad) tentang hari Kiamat, “Kapankah terjadinya?” ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/079042.mp3",
    "surat_id": 79,
    "juz_id": 30,
    "page_number": 584
  },
  {
    "ayat_id": 5755,
    "ayat_number": 43,
    "ayat_arabic": "فِيْمَ اَنْتَ مِنْ ذِكْرٰىهَاۗ",

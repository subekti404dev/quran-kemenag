[
  {
    "ayat_id": 5968,
    "ayat_number": 1,
    "ayat_arabic": "هَلْ اَتٰىكَ حَدِيْثُ الْغَاشِيَةِۗ",
    "ayat_bahasa": "Sudahkah sampai kepadamu berita tentang (hari Kiamat)?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/088001.mp3",
    "surat_id": 88,
    "juz_id": 30,
    "page_number": 592
  },
  {
    "ayat_id": 5969,
    "ayat_number": 2,
    "ayat_arabic": "وُجُوْهٌ يَّوْمَىِٕذٍ خَاشِعَةٌ  ۙ",
    "ayat_bahasa": "Pada hari itu banyak wajah yang tertunduk terhina,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/088002.mp3",
    "surat_id": 88,
    "juz_id": 30,
    "page_number": 592
  },
  {
    "ayat_id": 5970,
    "ayat_number": 3,
    "ayat_arabic": "عَامِلَةٌ نَّاصِبَةٌ  ۙ",
    "ayat_bahasa": "(karena) bekerja keras lagi kepayahan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/088003.mp3",
    "surat_id": 88,
    "juz_id": 30,
    "page_number": 592
  },
  {
    "ayat_id": 5971,
    "ayat_number": 4,
    "ayat_arabic": "تَصْلٰى نَارًا حَامِيَةً  ۙ",
    "ayat_bahasa": "mereka memasuki api yang sangat panas (neraka),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/088004.mp3",
    "surat_id": 88,
    "juz_id": 30,
    "page_number": 592
  },
  {
    "ayat_id": 5972,
    "ayat_number": 5,
    "ayat_arabic": "تُسْقٰى مِنْ عَيْنٍ اٰنِيَةٍ ۗ",
    "ayat_bahasa": "diberi minum dari sumber mata air yang sangat panas.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/088005.mp3",
    "surat_id": 88,
    "juz_id": 30,
    "page_number": 592
  },
  {
    "ayat_id": 5973,
    "ayat_number": 6,
    "ayat_arabic": "لَيْسَ لَهُمْ طَعَامٌ اِلَّا مِنْ ضَرِيْعٍۙ",
    "ayat_bahasa": "Tidak ada makanan bagi mereka selain dari pohon yang berduri,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/088006.mp3",
    "surat_id": 88,
    "juz_id": 30,
    "page_number": 592
  },
  {
    "ayat_id": 5974,
    "ayat_number": 7,
    "ayat_arabic": "لَّا يُسْمِنُ وَلَا يُغْنِيْ مِنْ جُوْعٍۗ",
    "ayat_bahasa": "yang tidak menggemukkan dan tidak menghilangkan lapar.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/088007.mp3",
    "surat_id": 88,
    "juz_id": 30,
    "page_number": 592
  },
  {
    "ayat_id": 5975,
    "ayat_number": 8,
    "ayat_arabic": "وُجُوْهٌ يَّوْمَىِٕذٍ نَّاعِمَةٌ  ۙ",
    "ayat_bahasa": "Pada hari itu banyak (pula) wajah yang berseri-seri,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/088008.mp3",
    "surat_id": 88,
    "juz_id": 30,
    "page_number": 592
  },
  {
    "ayat_id": 5976,
    "ayat_number": 9,
    "ayat_arabic": "لِّسَعْيِهَا رَاضِيَةٌ  ۙ ",
    "ayat_bahasa": "merasa senang karena usahanya (sendiri),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/088009.mp3",
    "surat_id": 88,
    "juz_id": 30,
    "page_number": 592
  },
  {
    "ayat_id": 5977,
    "ayat_number": 10,
    "ayat_arabic": "فِيْ جَنَّةٍ عَالِيَةٍۙ",
    "ayat_bahasa": "(mereka) dalam surga yang tinggi, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/088010.mp3",
    "surat_id": 88,
    "juz_id": 30,
    "page_number": 592
  },
  {
    "ayat_id": 5978,
    "ayat_number": 11,
    "ayat_arabic": "لَّا تَسْمَعُ فِيْهَا لَاغِيَةً ۗ",
    "ayat_bahasa": "di sana (kamu) tidak mendengar perkataan yang tidak berguna.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/088011.mp3",
    "surat_id": 88,
    "juz_id": 30,
    "page_number": 592
  },
  {
    "ayat_id": 5979,
    "ayat_number": 12,
    "ayat_arabic": "فِيْهَا عَيْنٌ جَارِيَةٌ  ۘ",
    "ayat_bahasa": "Di sana ada mata air yang mengalir.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/088012.mp3",
    "surat_id": 88,
    "juz_id": 30,
    "page_number": 592
  },
  {
    "ayat_id": 5980,
    "ayat_number": 13,
    "ayat_arabic": "فِيْهَا سُرُرٌ مَّرْفُوْعَةٌ  ۙ",
    "ayat_bahasa": "Di sana ada dipan-dipan yang ditinggikan,",

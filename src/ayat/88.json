    "surat_id": 88,
    "juz_id": 30,
    "page_number": 592
  },
  {
    "ayat_id": 5985,
    "ayat_number": 18,
    "ayat_arabic": "وَاِلَى السَّمَاۤءِ كَيْفَ رُفِعَتْۗ",

    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/088014.mp3",
    "surat_id": 88,
    "juz_id": 30,
    "page_number": 592
  },
  {
    "ayat_id": 5982,
    "ayat_number": 15,
    "ayat_arabic": "وَّنَمَارِقُ مَصْفُوْفَةٌ  ۙ ",
    "ayat_bahasa": "dan bantal-bantal sandaran yang tersusun,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/088015.mp3",
    "surat_id": 88,
    "juz_id": 30,
    "page_number": 592
  },
  {
    "ayat_id": 5983,
    "ayat_number": 16,
    "ayat_arabic": "وَّزَرَابِيُّ مَبْثُوْثَةٌ  ۗ",

[
  {
    "ayat_id": 5910,
    "ayat_number": 1,
    "ayat_arabic": "وَالسَّمَاۤءِ ذَاتِ الْبُرُوْجِۙ ",
    "ayat_bahasa": "Demi langit yang mempunyai gugusan bintang,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/085001.mp3",
    "surat_id": 85,
    "juz_id": 30,
    "page_number": 590
  },
  {
    "ayat_id": 5911,
    "ayat_number": 2,
    "ayat_arabic": "وَالْيَوْمِ الْمَوْعُوْدِۙ ",
    "ayat_bahasa": "dan demi hari yang dijanjikan.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/085002.mp3",
    "surat_id": 85,
    "juz_id": 30,
    "page_number": 590
  },
  {
    "ayat_id": 5912,
    "ayat_number": 3,
    "ayat_arabic": "وَشَاهِدٍ وَّمَشْهُوْدٍۗ ",
    "ayat_bahasa": "Demi yang menyaksikan dan yang disaksikan.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/085003.mp3",
    "surat_id": 85,
    "juz_id": 30,
    "page_number": 590
  },
  {
    "ayat_id": 5913,
    "ayat_number": 4,
    "ayat_arabic": "قُتِلَ اَصْحٰبُ الْاُخْدُوْدِۙ ",
    "ayat_bahasa": "Binasalah orang-orang yang membuat parit (yaitu para pembesar Najran di Yaman),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/085004.mp3",
    "surat_id": 85,
    "juz_id": 30,
    "page_number": 590
  },
  {
    "ayat_id": 5914,
    "ayat_number": 5,
    "ayat_arabic": "النَّارِ ذَاتِ الْوَقُوْدِۙ ",
    "ayat_bahasa": "yang berapi (yang mempunyai) kayu bakar,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/085005.mp3",
    "surat_id": 85,
    "juz_id": 30,
    "page_number": 590
  },
  {
    "ayat_id": 5915,
    "ayat_number": 6,
    "ayat_arabic": "اِذْ هُمْ عَلَيْهَا قُعُوْدٌۙ ",
    "ayat_bahasa": "ketika mereka duduk di sekitarnya, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/085006.mp3",
    "surat_id": 85,
    "juz_id": 30,
    "page_number": 590
  },
  {
    "ayat_id": 5916,
    "ayat_number": 7,
    "ayat_arabic": "وَّهُمْ عَلٰى مَا يَفْعَلُوْنَ بِالْمُؤْمِنِيْنَ شُهُوْدٌ  ۗ ",
    "ayat_bahasa": "sedang mereka menyaksikan apa yang mereka perbuat terhadap orang-orang mukmin.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/085007.mp3",
    "surat_id": 85,
    "juz_id": 30,
    "page_number": 590
  },
  {
    "ayat_id": 5917,
    "ayat_number": 8,
    "ayat_arabic": "وَمَا نَقَمُوْا مِنْهُمْ اِلَّآ اَنْ يُّؤْمِنُوْا بِاللّٰهِ الْعَزِيْزِ الْحَمِيْدِۙ ",
    "ayat_bahasa": "Dan mereka menyiksa orang-orang mukmin itu hanya karena (orang-orang mukmin itu) beriman kepada Allah Yang Mahaperkasa, Maha Terpuji,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/085008.mp3",
    "surat_id": 85,
    "juz_id": 30,
    "page_number": 590
  },
  {
    "ayat_id": 5918,
    "ayat_number": 9,
    "ayat_arabic": "الَّذِيْ لَهٗ مُلْكُ السَّمٰوٰتِ وَالْاَرْضِ ۗوَاللّٰهُ عَلٰى كُلِّ شَيْءٍ شَهِيْدٌ  ۗ ",
    "ayat_bahasa": "yang memiliki kerajaan langit dan bumi. Dan Allah Maha Menyaksikan segala sesuatu.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/085009.mp3",
    "surat_id": 85,
    "juz_id": 30,
    "page_number": 590
  },
  {
    "ayat_id": 5919,
    "ayat_number": 10,
    "ayat_arabic": "اِنَّ الَّذِيْنَ فَتَنُوا الْمُؤْمِنِيْنَ وَالْمُؤْمِنٰتِ ثُمَّ لَمْ يَتُوْبُوْا فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ الْحَرِيْقِۗ ",
    "ayat_bahasa": "Sungguh, orang-orang yang mendatangkan cobaan (bencana, membunuh, menyiksa) kepada orang-orang mukmin laki-laki dan perempuan lalu mereka tidak bertobat, maka mereka akan mendapat azab Jahanam dan mereka akan mendapat azab (neraka) yang membakar.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/085010.mp3",
    "surat_id": 85,
    "juz_id": 30,
    "page_number": 590
  },
  {
    "ayat_id": 5920,
    "ayat_number": 11,
    "ayat_arabic": "اِنَّ الَّذِيْنَ اٰمَنُوْا وَعَمِلُوا الصّٰلِحٰتِ لَهُمْ جَنّٰتٌ تَجْرِيْ مِنْ تَحْتِهَا الْاَنْهٰرُ ەۗ ذٰلِكَ الْفَوْزُ الْكَبِيْرُۗ",
    "ayat_bahasa": "Sungguh, orang-orang yang beriman dan mengerjakan kebajikan, mereka akan mendapat surga yang mengalir di bawahnya sungai-sungai, itulah kemenangan yang agung.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/085011.mp3",
    "surat_id": 85,
    "juz_id": 30,
    "page_number": 590
  },
  {
    "ayat_id": 5921,
    "ayat_number": 12,
    "ayat_arabic": "اِنَّ بَطْشَ رَبِّكَ لَشَدِيْدٌ ۗ ",
    "ayat_bahasa": "Sungguh, azab Tuhanmu sangat keras.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/085012.mp3",
    "surat_id": 85,
    "juz_id": 30,
    "page_number": 590
  },
  {
    "ayat_id": 5922,
    "ayat_number": 13,
    "ayat_arabic": "اِنَّهٗ هُوَ يُبْدِئُ وَيُعِيْدُۚ ",
    "ayat_bahasa": "Sungguh, Dialah yang memulai pen-ciptaan (makhluk) dan yang menghidupkannya (kembali).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/085013.mp3",
    "surat_id": 85,
    "juz_id": 30,
    "page_number": 590
  },
  {
    "ayat_id": 5923,
    "ayat_number": 14,
    "ayat_arabic": "وَهُوَ الْغَفُوْرُ الْوَدُوْدُۙ ",
    "ayat_bahasa": "Dan Dialah Yang Maha Pengampun, Maha Pengasih,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/085014.mp3",
    "surat_id": 85,
    "juz_id": 30,
    "page_number": 590
  },
  {
    "ayat_id": 5924,
    "ayat_number": 15,
    "ayat_arabic": "ذُو الْعَرْشِ الْمَجِيْدُۙ ",
    "ayat_bahasa": "yang memiliki ‘Arsy, lagi Mahamulia, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/085015.mp3",
    "surat_id": 85,
    "juz_id": 30,
    "page_number": 590
  },
  {
    "ayat_id": 5925,

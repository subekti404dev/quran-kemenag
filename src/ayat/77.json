[
  {
    "ayat_id": 5623,
    "ayat_number": 1,
    "ayat_arabic": "وَالْمُرْسَلٰتِ عُرْفًاۙ",
    "ayat_bahasa": "Demi (malaikat-malaikat) yang diutus untuk membawa kebaikan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077001.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5624,
    "ayat_number": 2,
    "ayat_arabic": "فَالْعٰصِفٰتِ عَصْفًاۙ",
    "ayat_bahasa": "dan (malaikat-malaikat) yang terbang dengan kencangnya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077002.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5625,
    "ayat_number": 3,
    "ayat_arabic": "وَّالنّٰشِرٰتِ نَشْرًاۙ",
    "ayat_bahasa": "dan (malaikat-malaikat) yang menyebarkan (rahmat Allah) dengan seluas-luasnya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077003.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5626,
    "ayat_number": 4,
    "ayat_arabic": "فَالْفٰرِقٰتِ فَرْقًاۙ",
    "ayat_bahasa": "dan (malaikat-malaikat) yang membedakan (antara yang baik dan yang buruk) dengan sejelas-jelasnya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077004.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5627,
    "ayat_number": 5,
    "ayat_arabic": "فَالْمُلْقِيٰتِ ذِكْرًاۙ",
    "ayat_bahasa": "dan (malaikat-malaikat) yang menyampaikan wahyu,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077005.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5628,
    "ayat_number": 6,
    "ayat_arabic": "عُذْرًا اَوْ نُذْرًاۙ",
    "ayat_bahasa": "untuk menolak alasan-alasan atau memberi peringatan.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077006.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5629,
    "ayat_number": 7,
    "ayat_arabic": "اِنَّمَا تُوْعَدُوْنَ لَوَاقِعٌۗ",
    "ayat_bahasa": "Sungguh, apa yang dijanjikan kepadamu pasti terjadi.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077007.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5630,
    "ayat_number": 8,
    "ayat_arabic": "فَاِذَا النُّجُوْمُ طُمِسَتْۙ",
    "ayat_bahasa": "Maka apabila bintang-bintang dihapuskan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077008.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5631,
    "ayat_number": 9,
    "ayat_arabic": "وَاِذَا السَّمَاۤءُ فُرِجَتْۙ",
    "ayat_bahasa": "dan apabila langit terbelah,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077009.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5632,
    "ayat_number": 10,
    "ayat_arabic": "وَاِذَا الْجِبَالُ نُسِفَتْۙ",
    "ayat_bahasa": "dan apabila gunung-gunung dihancurkan menjadi debu,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077010.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5633,
    "ayat_number": 11,
    "ayat_arabic": "وَاِذَا الرُّسُلُ اُقِّتَتْۗ",
    "ayat_bahasa": "dan apabila rasul-rasul telah ditetapkan waktunya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077011.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5634,
    "ayat_number": 12,
    "ayat_arabic": "لِاَيِّ يَوْمٍ اُجِّلَتْۗ",
    "ayat_bahasa": "(Niscaya dikatakan kepada mereka), “Sampai hari apakah ditangguhkan (azab orang-orang kafir itu)?”",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077012.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5635,
    "ayat_number": 13,
    "ayat_arabic": "لِيَوْمِ الْفَصْلِۚ",
    "ayat_bahasa": "Sampai hari keputusan.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077013.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5636,
    "ayat_number": 14,
    "ayat_arabic": "وَمَآ اَدْرٰىكَ مَا يَوْمُ الْفَصْلِۗ",
    "ayat_bahasa": "Dan tahukah kamu apakah hari ke-putusan itu?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077014.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5637,
    "ayat_number": 15,
    "ayat_arabic": "وَيْلٌ يَّوْمَىِٕذٍ لِّلْمُكَذِّبِيْنَ ",
    "ayat_bahasa": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077015.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5638,
    "ayat_number": 16,
    "ayat_arabic": "اَلَمْ نُهْلِكِ الْاَوَّلِيْنَۗ",
    "ayat_bahasa": "Bukankah telah Kami binasakan orang-orang yang dahulu?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077016.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5639,
    "ayat_number": 17,
    "ayat_arabic": "ثُمَّ نُتْبِعُهُمُ الْاٰخِرِيْنَ ",
    "ayat_bahasa": "Lalu Kami susulkan (azab Kami terhadap) orang-orang yang datang kemudian.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077017.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5640,
    "ayat_number": 18,
    "ayat_arabic": "كَذٰلِكَ نَفْعَلُ بِالْمُجْرِمِيْنَ ",
    "ayat_bahasa": "Demikianlah Kami perlakukan orang-orang yang berdosa.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077018.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5641,
    "ayat_number": 19,
    "ayat_arabic": "وَيْلٌ يَّوْمَىِٕذٍ لِّلْمُكَذِّبِيْنَ ",
    "ayat_bahasa": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077019.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 580
  },
  {
    "ayat_id": 5642,
    "ayat_number": 20,
    "ayat_arabic": "اَلَمْ نَخْلُقْكُّمْ مِّنْ مَّاۤءٍ مَّهِيْنٍۙ",
    "ayat_bahasa": "Bukankah Kami menciptakan kamu dari air yang hina (mani),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077020.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5643,
    "ayat_number": 21,
    "ayat_arabic": "فَجَعَلْنٰهُ فِيْ قَرَارٍ مَّكِيْنٍ",
    "ayat_bahasa": "kemudian Kami letakkan ia dalam tempat yang kokoh (rahim),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077021.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5644,
    "ayat_number": 22,
    "ayat_arabic": "اِلٰى قَدَرٍ مَّعْلُوْمٍۙ",
    "ayat_bahasa": "sampai waktu yang ditentukan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077022.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5645,
    "ayat_number": 23,
    "ayat_arabic": "فَقَدَرْنَاۖ فَنِعْمَ الْقٰدِرُوْنَ ",
    "ayat_bahasa": "lalu Kami tentukan (bentuknya), maka (Kamilah) sebaik-baik yang menentukan.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077023.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5646,
    "ayat_number": 24,
    "ayat_arabic": "وَيْلٌ يَّوْمَىِٕذٍ لِّلْمُكَذِّبِيْنَ ",
    "ayat_bahasa": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077024.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5647,
    "ayat_number": 25,
    "ayat_arabic": "اَلَمْ نَجْعَلِ الْاَرْضَ كِفَاتًاۙ",
    "ayat_bahasa": "Bukankah Kami jadikan bumi untuk (tempat) berkumpul,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077025.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5648,
    "ayat_number": 26,
    "ayat_arabic": "اَحْيَاۤءً وَّاَمْوَاتًاۙ",
    "ayat_bahasa": "bagi yang masih hidup dan yang sudah mati?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077026.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5649,
    "ayat_number": 27,
    "ayat_arabic": "وَّجَعَلْنَا فِيْهَا رَوَاسِيَ شٰمِخٰتٍ وَّاَسْقَيْنٰكُمْ مَّاۤءً فُرَاتًاۗ",
    "ayat_bahasa": "Dan Kami jadikan padanya gunung-gunung yang tinggi, dan Kami beri minum kamu dengan air tawar?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077027.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5650,
    "ayat_number": 28,
    "ayat_arabic": "وَيْلٌ يَّوْمَىِٕذٍ لِّلْمُكَذِّبِيْنَ ",
    "ayat_bahasa": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077028.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5651,
    "ayat_number": 29,
    "ayat_arabic": "اِنْطَلِقُوْٓا اِلٰى مَا كُنْتُمْ بِهٖ تُكَذِّبُوْنَۚ",
    "ayat_bahasa": "(Akan dikatakan), “Pergilah kamu mendapatkan apa (azab) yang dahulu kamu dustakan.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077029.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5652,
    "ayat_number": 30,
    "ayat_arabic": "اِنْطَلِقُوْٓا اِلٰى ظِلٍّ ذِيْ ثَلٰثِ شُعَبٍ ",
    "ayat_bahasa": "Pergilah kamu mendapatkan naungan (asap api neraka) yang mempunyai tiga cabang, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077030.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5653,
    "ayat_number": 31,
    "ayat_arabic": "لَا ظَلِيْلٍ وَّلَا يُغْنِيْ مِنَ اللَّهَبِۗ",
    "ayat_bahasa": "yang tidak melindungi dan tidak pula menolak nyala api neraka.” ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077031.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5654,
    "ayat_number": 32,
    "ayat_arabic": "اِنَّهَا تَرْمِيْ بِشَرَرٍ كَالْقَصْرِۚ",
    "ayat_bahasa": "Sungguh, (neraka) itu menyemburkan bunga api (sebesar dan setinggi) istana, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077032.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5655,
    "ayat_number": 33,
    "ayat_arabic": "كَاَنَّهٗ جِمٰلَتٌ صُفْرٌۗ",
    "ayat_bahasa": "seakan-akan iring-iringan unta yang kuning.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077033.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5656,
    "ayat_number": 34,
    "ayat_arabic": "وَيْلٌ يَّوْمَىِٕذٍ لِّلْمُكَذِّبِيْنَ ",
    "ayat_bahasa": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran). ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077034.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5657,
    "ayat_number": 35,
    "ayat_arabic": "هٰذَا يَوْمُ لَا يَنْطِقُوْنَۙ",
    "ayat_bahasa": "Inilah hari, saat mereka tidak dapat berbicara, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077035.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5658,
    "ayat_number": 36,
    "ayat_arabic": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُوْنَ ",
    "ayat_bahasa": "dan tidak diizinkan kepada mereka mengemukakan alasan agar mereka dimaafkan. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077036.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5659,
    "ayat_number": 37,
    "ayat_arabic": "وَيْلٌ يَّوْمَىِٕذٍ لِّلْمُكَذِّبِيْنَ ",
    "ayat_bahasa": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran). ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077037.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5660,
    "ayat_number": 38,
    "ayat_arabic": "هٰذَا يَوْمُ الْفَصْلِ جَمَعْنٰكُمْ وَالْاَوَّلِيْنَ ",
    "ayat_bahasa": "Inilah hari keputusan; (pada hari ini) Kami kumpulkan kamu dan orang-orang yang terdahulu. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077038.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5661,
    "ayat_number": 39,
    "ayat_arabic": "فَاِنْ كَانَ لَكُمْ كَيْدٌ فَكِيْدُوْنِ ",
    "ayat_bahasa": "Maka jika kamu punya tipu daya, maka lakukanlah (tipu daya) itu terhadap-Ku. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077039.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5662,
    "ayat_number": 40,
    "ayat_arabic": "وَيْلٌ يَّوْمَىِٕذٍ لِّلْمُكَذِّبِيْنَ ࣖ ",
    "ayat_bahasa": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077040.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5663,
    "ayat_number": 41,
    "ayat_arabic": "اِنَّ الْمُتَّقِيْنَ فِيْ ظِلٰلٍ وَّعُيُوْنٍۙ",
    "ayat_bahasa": "Sungguh, orang-orang yang bertakwa berada dalam naungan (pepohonan surga yang teduh) dan (di sekitar) mata air, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077041.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {
    "ayat_id": 5664,
    "ayat_number": 42,
    "ayat_arabic": "وَّفَوَاكِهَ مِمَّا يَشْتَهُوْنَۗ",
    "ayat_bahasa": "dan buah-buahan yang mereka sukai. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/077042.mp3",
    "surat_id": 77,
    "juz_id": 29,
    "page_number": 581
  },
  {

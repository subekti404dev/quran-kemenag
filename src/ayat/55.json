[
  {
    "ayat_id": 4902,
    "ayat_number": 1,
    "ayat_arabic": "اَلرَّحْمٰنُۙ  ",
    "ayat_bahasa": "(Allah) Yang Maha Pengasih,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/055001.mp3",
    "surat_id": 55,
    "juz_id": 27,
    "page_number": 531
  },
  {
    "ayat_id": 4903,
    "ayat_number": 2,
    "ayat_arabic": "عَلَّمَ الْقُرْاٰنَۗ ",
    "ayat_bahasa": "Yang telah mengajarkan Al-Qur'an.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/055002.mp3",
    "surat_id": 55,
    "juz_id": 27,
    "page_number": 531
  },
  {
    "ayat_id": 4904,
    "ayat_number": 3,
    "ayat_arabic": "خَلَقَ الْاِنْسَانَۙ ",
    "ayat_bahasa": "Dia menciptakan manusia, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/055003.mp3",
    "surat_id": 55,
    "juz_id": 27,
    "page_number": 531
  },
  {
    "ayat_id": 4905,
    "ayat_number": 4,
    "ayat_arabic": "عَلَّمَهُ الْبَيَانَ ",
    "ayat_bahasa": "mengajarnya pandai berbicara.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/055004.mp3",
    "surat_id": 55,
    "juz_id": 27,
    "page_number": 531
  },
  {
    "ayat_id": 4906,
    "ayat_number": 5,
    "ayat_arabic": "اَلشَّمْسُ وَالْقَمَرُ بِحُسْبَانٍۙ ",
    "ayat_bahasa": "Matahari dan bulan beredar menurut perhitungan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/055005.mp3",
    "surat_id": 55,
    "juz_id": 27,
    "page_number": 531
  },
  {
    "ayat_id": 4907,
    "ayat_number": 6,
    "ayat_arabic": "وَّالنَّجْمُ وَالشَّجَرُ يَسْجُدَانِ ",
    "ayat_bahasa": "dan tetumbuhan dan pepohonan, keduanya tunduk (kepada-Nya). ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/055006.mp3",
    "surat_id": 55,
    "juz_id": 27,
    "page_number": 531
  },
  {
    "ayat_id": 4908,
    "ayat_number": 7,
    "ayat_arabic": "وَالسَّمَاۤءَ رَفَعَهَا وَوَضَعَ الْمِيْزَانَۙ  ",
    "ayat_bahasa": "Dan langit telah ditinggikan-Nya dan Dia ciptakan keseimbangan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/055007.mp3",
    "surat_id": 55,
    "juz_id": 27,
    "page_number": 531
  },
  {
    "ayat_id": 4909,
    "ayat_number": 8,
    "ayat_arabic": "اَلَّا تَطْغَوْا فِى الْمِيْزَانِ ",
    "ayat_bahasa": "agar kamu jangan merusak keseimbangan itu, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/055008.mp3",
    "surat_id": 55,
    "juz_id": 27,
    "page_number": 531
  },
  {
    "ayat_id": 4910,
    "ayat_number": 9,
    "ayat_arabic": "وَاَقِيْمُوا الْوَزْنَ بِالْقِسْطِ وَلَا تُخْسِرُوا الْمِيْزَانَ ",
    "ayat_bahasa": "dan tegakkanlah keseimbangan itu dengan adil dan janganlah kamu mengurangi keseimbangan itu. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/055009.mp3",
    "surat_id": 55,
    "juz_id": 27,
    "page_number": 531
  },
  {
    "ayat_id": 4911,
    "ayat_number": 10,
    "ayat_arabic": "وَالْاَرْضَ وَضَعَهَا لِلْاَنَامِۙ  ",
    "ayat_bahasa": "Dan bumi telah dibentangkan-Nya untuk makhluk(-Nya), ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/055010.mp3",
    "surat_id": 55,
    "juz_id": 27,
    "page_number": 531
  },
  {
    "ayat_id": 4912,
    "ayat_number": 11,
    "ayat_arabic": "فِيْهَا فَاكِهَةٌ وَّالنَّخْلُ ذَاتُ الْاَكْمَامِۖ  ",
    "ayat_bahasa": "di dalamnya ada buah-buahan dan pohon kurma yang mempunyai kelopak mayang, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/055011.mp3",
    "surat_id": 55,
    "juz_id": 27,
    "page_number": 531
  },
  {
    "ayat_id": 4913,
    "ayat_number": 12,
    "ayat_arabic": "وَالْحَبُّ ذُو الْعَصْفِ وَالرَّيْحَانُۚ  ",
    "ayat_bahasa": "dan biji-bijian yang berkulit dan bunga-bunga yang harum baunya. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/055012.mp3",
    "surat_id": 55,
    "juz_id": 27,
    "page_number": 531
  },
  {
    "ayat_id": 4914,
    "ayat_number": 13,
    "ayat_arabic": "فَبِاَيِّ اٰلَاۤءِ رَبِّكُمَا تُكَذِّبٰنِ ",
    "ayat_bahasa": "Maka nikmat Tuhanmu yang manakah yang kamu dustakan? ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/055013.mp3",
    "surat_id": 55,
    "juz_id": 27,
    "page_number": 531
  },
  {
    "ayat_id": 4915,
    "ayat_number": 14,
    "ayat_arabic": "خَلَقَ الْاِنْسَانَ مِنْ صَلْصَالٍ كَالْفَخَّارِ ",
    "ayat_bahasa": "Dia menciptakan manusia dari tanah kering seperti tembikar,",

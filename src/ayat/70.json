[
  {
    "ayat_id": 5376,
    "ayat_number": 1,
    "ayat_arabic": "سَاَلَ سَاۤىِٕلٌۢ بِعَذَابٍ وَّاقِعٍۙ",
    "ayat_bahasa": "Seseorang bertanya tentang azab yang pasti terjadi, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070001.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 568
  },
  {
    "ayat_id": 5377,
    "ayat_number": 2,
    "ayat_arabic": "لِّلْكٰفِرِيْنَ لَيْسَ لَهٗ دَافِعٌۙ",
    "ayat_bahasa": "bagi orang-orang kafir, yang tidak seorang pun dapat menolaknya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070002.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 568
  },
  {
    "ayat_id": 5378,
    "ayat_number": 3,
    "ayat_arabic": "مِّنَ اللّٰهِ ذِى الْمَعَارِجِۗ",
    "ayat_bahasa": "(Azab) dari Allah, yang memiliki tempat-tempat naik.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070003.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 568
  },
  {
    "ayat_id": 5379,
    "ayat_number": 4,
    "ayat_arabic": "تَعْرُجُ الْمَلٰۤىِٕكَةُ وَالرُّوْحُ اِلَيْهِ فِيْ يَوْمٍ كَانَ مِقْدَارُهٗ خَمْسِيْنَ اَلْفَ سَنَةٍۚ",
    "ayat_bahasa": "Para malaikat dan Jibril naik (menghadap) kepada Tuhan, dalam sehari setara dengan lima puluh ribu tahun.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070004.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 568
  },
  {
    "ayat_id": 5380,
    "ayat_number": 5,
    "ayat_arabic": "فَاصْبِرْ صَبْرًا جَمِيْلًا",
    "ayat_bahasa": "Maka bersabarlah engkau (Muhammad) dengan kesabaran yang baik.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070005.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 568
  },
  {
    "ayat_id": 5381,
    "ayat_number": 6,
    "ayat_arabic": "اِنَّهُمْ يَرَوْنَهٗ بَعِيْدًاۙ",
    "ayat_bahasa": "Mereka memandang (azab) itu jauh (mustahil). ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070006.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 568
  },
  {
    "ayat_id": 5382,
    "ayat_number": 7,
    "ayat_arabic": "وَّنَرٰىهُ قَرِيْبًاۗ",
    "ayat_bahasa": "Sedangkan Kami memandangnya dekat (pasti terjadi). ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070007.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 568
  },
  {
    "ayat_id": 5383,
    "ayat_number": 8,
    "ayat_arabic": "يَوْمَ تَكُوْنُ السَّمَاۤءُ كَالْمُهْلِۙ",
    "ayat_bahasa": "(Ingatlah) pada hari ketika langit men-jadi bagaikan cairan tembaga, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070008.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 568
  },
  {
    "ayat_id": 5384,
    "ayat_number": 9,
    "ayat_arabic": "وَتَكُوْنُ الْجِبَالُ كَالْعِهْنِۙ",
    "ayat_bahasa": "dan gunung-gunung bagaikan bulu (yang beterbangan), ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070009.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 568
  },
  {
    "ayat_id": 5385,
    "ayat_number": 10,
    "ayat_arabic": "وَلَا يَسْـَٔلُ حَمِيْمٌ حَمِيْمًاۚ ",
    "ayat_bahasa": "dan tidak ada seorang teman karib pun menanyakan temannya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070010.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 568
  },
  {
    "ayat_id": 5386,
    "ayat_number": 11,
    "ayat_arabic": "يُبَصَّرُوْنَهُمْۗ يَوَدُّ الْمُجْرِمُ لَوْ يَفْتَدِيْ مِنْ عَذَابِ يَوْمِىِٕذٍۢ بِبَنِيْهِۙ",
    "ayat_bahasa": "sedang mereka saling melihat. Pada hari itu, orang yang berdosa ingin sekiranya dia dapat menebus (dirinya) dari azab dengan anak-anaknya, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070011.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5387,
    "ayat_number": 12,
    "ayat_arabic": "وَصَاحِبَتِهٖ وَاَخِيْهِۙ",
    "ayat_bahasa": "dan istrinya dan saudaranya, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070012.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5388,
    "ayat_number": 13,
    "ayat_arabic": "وَفَصِيْلَتِهِ الَّتِيْ تُـْٔوِيْهِۙ",
    "ayat_bahasa": "dan keluarga yang melindunginya (di dunia),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070013.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5389,
    "ayat_number": 14,
    "ayat_arabic": "وَمَنْ فِى الْاَرْضِ جَمِيْعًاۙ ثُمَّ يُنْجِيْهِۙ",
    "ayat_bahasa": "dan orang-orang di bumi seluruhnya, kemudian mengharapkan (tebusan) itu dapat menyelamatkannya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070014.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5390,
    "ayat_number": 15,
    "ayat_arabic": "كَلَّاۗ اِنَّهَا لَظٰىۙ",
    "ayat_bahasa": "Sama sekali tidak! Sungguh, neraka itu api yang bergejolak,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070015.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5391,
    "ayat_number": 16,
    "ayat_arabic": "نَزَّاعَةً لِّلشَّوٰىۚ",
    "ayat_bahasa": "yang mengelupaskan kulit kepala. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070016.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5392,
    "ayat_number": 17,
    "ayat_arabic": "تَدْعُوْا مَنْ اَدْبَرَ وَتَوَلّٰىۙ",
    "ayat_bahasa": "Yang memanggil orang yang membelakangi dan yang berpaling (dari agama),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070017.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5393,
    "ayat_number": 18,
    "ayat_arabic": "وَجَمَعَ فَاَوْعٰى ",
    "ayat_bahasa": "dan orang yang mengumpulkan (harta benda) lalu menyimpannya. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070018.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5394,
    "ayat_number": 19,
    "ayat_arabic": "۞ اِنَّ الْاِنْسَانَ خُلِقَ هَلُوْعًاۙ",
    "ayat_bahasa": "Sungguh, manusia diciptakan bersifat suka mengeluh. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070019.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5395,
    "ayat_number": 20,
    "ayat_arabic": "اِذَا مَسَّهُ الشَّرُّ جَزُوْعًاۙ",
    "ayat_bahasa": "Apabila dia ditimpa kesusahan dia berkeluh kesah, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070020.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5396,
    "ayat_number": 21,
    "ayat_arabic": "وَّاِذَا مَسَّهُ الْخَيْرُ مَنُوْعًاۙ",
    "ayat_bahasa": "dan apabila mendapat kebaikan (harta) dia jadi kikir, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070021.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5397,
    "ayat_number": 22,
    "ayat_arabic": "اِلَّا الْمُصَلِّيْنَۙ",
    "ayat_bahasa": "kecuali orang-orang yang melaksanakan salat, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070022.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5398,
    "ayat_number": 23,
    "ayat_arabic": "الَّذِيْنَ هُمْ عَلٰى صَلَاتِهِمْ دَاۤىِٕمُوْنَۖ",
    "ayat_bahasa": "mereka yang tetap setia melaksanakan salatnya, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070023.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5399,
    "ayat_number": 24,
    "ayat_arabic": "وَالَّذِيْنَ فِيْٓ اَمْوَالِهِمْ حَقٌّ مَّعْلُوْمٌۖ",
    "ayat_bahasa": "dan orang-orang yang dalam hartanya disiapkan bagian tertentu, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070024.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5400,
    "ayat_number": 25,
    "ayat_arabic": "لِّلسَّاۤىِٕلِ وَالْمَحْرُوْمِۖ",
    "ayat_bahasa": "bagi orang (miskin) yang meminta dan yang tidak meminta, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070025.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5401,
    "ayat_number": 26,
    "ayat_arabic": "وَالَّذِيْنَ يُصَدِّقُوْنَ بِيَوْمِ الدِّيْنِۖ",
    "ayat_bahasa": "dan orang-orang yang mempercayai hari pembalasan, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070026.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5402,
    "ayat_number": 27,
    "ayat_arabic": "وَالَّذِيْنَ هُمْ مِّنْ عَذَابِ رَبِّهِمْ مُّشْفِقُوْنَۚ",
    "ayat_bahasa": "dan orang-orang yang takut terhadap azab Tuhannya, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070027.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5403,
    "ayat_number": 28,
    "ayat_arabic": "اِنَّ عَذَابَ رَبِّهِمْ غَيْرُ مَأْمُوْنٍۖ",
    "ayat_bahasa": "sesungguhnya terhadap azab Tuhan mereka, tidak ada seseorang yang merasa aman (dari kedatangannya), ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070028.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5404,
    "ayat_number": 29,
    "ayat_arabic": "وَّالَّذِيْنَ هُمْ لِفُرُوْجِهِمْ حٰفِظُوْنَۙ",
    "ayat_bahasa": "dan orang-orang yang memelihara kemaluannya, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070029.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5405,
    "ayat_number": 30,
    "ayat_arabic": "اِلَّا عَلٰٓى اَزْوَاجِهِمْ اَوْ مَا مَلَكَتْ اَيْمَانُهُمْ فَاِنَّهُمْ غَيْرُ مَلُوْمِيْنَۚ",
    "ayat_bahasa": "kecuali terhadap istri-istri mereka atau hamba sahaya yang mereka miliki maka sesungguhnya mereka tidak tercela. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070030.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5406,
    "ayat_number": 31,
    "ayat_arabic": "فَمَنِ ابْتَغٰى وَرَاۤءَ ذٰلِكَ فَاُولٰۤىِٕكَ هُمُ الْعٰدُوْنَۚ",
    "ayat_bahasa": "Maka barangsiapa mencari di luar itu (seperti zina, homoseks dan lesbian), mereka itulah orang-orang yang melampaui batas. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070031.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5407,
    "ayat_number": 32,
    "ayat_arabic": "وَالَّذِيْنَ هُمْ لِاَمٰنٰتِهِمْ وَعَهْدِهِمْ رَاعُوْنَۖ",
    "ayat_bahasa": "Dan orang-orang yang memelihara amanat dan janjinya, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070032.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5408,
    "ayat_number": 33,
    "ayat_arabic": "وَالَّذِيْنَ هُمْ بِشَهٰدٰتِهِمْ قَاۤىِٕمُوْنَۖ",
    "ayat_bahasa": "dan orang-orang yang berpegang teguh pada kesaksiannya, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070033.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5409,
    "ayat_number": 34,
    "ayat_arabic": "وَالَّذِيْنَ هُمْ عَلٰى صَلَاتِهِمْ يُحَافِظُوْنَۖ",
    "ayat_bahasa": "dan orang-orang yang memelihara salatnya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070034.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5410,
    "ayat_number": 35,
    "ayat_arabic": "اُولٰۤىِٕكَ فِيْ جَنّٰتٍ مُّكْرَمُوْنَ ۗ ࣖ",
    "ayat_bahasa": "Mereka itu dimuliakan di dalam surga. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070035.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5411,
    "ayat_number": 36,
    "ayat_arabic": "فَمَالِ الَّذِيْنَ كَفَرُوْا قِبَلَكَ مُهْطِعِيْنَۙ",
    "ayat_bahasa": "Maka mengapa orang-orang kafir itu datang bergegas ke hadapanmu (Muhammad), ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070036.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5412,
    "ayat_number": 37,
    "ayat_arabic": "عَنِ الْيَمِيْنِ وَعَنِ الشِّمَالِ عِزِيْنَ ",
    "ayat_bahasa": "dari kanan dan dari kiri dengan berkelompok-kelompok?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070037.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5413,
    "ayat_number": 38,
    "ayat_arabic": "اَيَطْمَعُ كُلُّ امْرِئٍ مِّنْهُمْ اَنْ يُّدْخَلَ جَنَّةَ نَعِيْمٍۙ",
    "ayat_bahasa": "Apakah setiap orang dari orang-orang kafir itu ingin masuk surga yang penuh kenikmatan?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070038.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5414,
    "ayat_number": 39,
    "ayat_arabic": "كَلَّاۗ اِنَّا خَلَقْنٰهُمْ مِّمَّا يَعْلَمُوْنَ ",
    "ayat_bahasa": "Tidak mungkin! Sesungguhnya Kami menciptakan mereka dari apa yang mereka ketahui.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070039.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 569
  },
  {
    "ayat_id": 5415,
    "ayat_number": 40,
    "ayat_arabic": "فَلَآ اُقْسِمُ بِرَبِّ الْمَشَارِقِ وَالْمَغٰرِبِ اِنَّا لَقٰدِرُوْنَۙ",
    "ayat_bahasa": "Maka Aku bersumpah demi Tuhan yang mengatur tempat-tempat terbit dan terbenamnya  (matahari, bulan dan bintang), sungguh, Kami pasti mampu, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/070040.mp3",
    "surat_id": 70,
    "juz_id": 29,
    "page_number": 570
  },
  {
    "ayat_id": 5416,
    "ayat_number": 41,

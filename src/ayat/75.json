[
  {
    "ayat_id": 5552,
    "ayat_number": 1,
    "ayat_arabic": "لَآ اُقْسِمُ بِيَوْمِ الْقِيٰمَةِۙ",
    "ayat_bahasa": "Aku bersumpah dengan hari Kiamat,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075001.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5553,
    "ayat_number": 2,
    "ayat_arabic": "وَلَآ اُقْسِمُ بِالنَّفْسِ اللَّوَّامَةِ ",
    "ayat_bahasa": "dan aku bersumpah demi jiwa yang selalu menyesali (dirinya sendiri).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075002.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5554,
    "ayat_number": 3,
    "ayat_arabic": "اَيَحْسَبُ الْاِنْسَانُ اَلَّنْ نَّجْمَعَ عِظَامَهٗ ۗ",
    "ayat_bahasa": "Apakah manusia mengira bahwa Kami tidak akan mengumpulkan (kembali) tulang-belulangnya?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075003.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5555,
    "ayat_number": 4,
    "ayat_arabic": "بَلٰى قَادِرِيْنَ عَلٰٓى اَنْ نُّسَوِّيَ بَنَانَهٗ ",
    "ayat_bahasa": "(Bahkan) Kami mampu menyusun (kembali) jari-jemarinya dengan sempurna. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075004.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5556,
    "ayat_number": 5,
    "ayat_arabic": "بَلْ يُرِيْدُ الْاِنْسَانُ لِيَفْجُرَ اَمَامَهٗۚ",
    "ayat_bahasa": "Tetapi manusia hendak membuat maksiat terus-menerus. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075005.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5557,
    "ayat_number": 6,
    "ayat_arabic": "يَسْـَٔلُ اَيَّانَ يَوْمُ الْقِيٰمَةِۗ",
    "ayat_bahasa": "Dia bertanya, “Kapankah hari Kiamat itu?”",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075006.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5558,
    "ayat_number": 7,
    "ayat_arabic": "فَاِذَا بَرِقَ الْبَصَرُۙ",
    "ayat_bahasa": "Maka apabila mata terbelalak (ketakutan), ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075007.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5559,
    "ayat_number": 8,
    "ayat_arabic": "وَخَسَفَ الْقَمَرُۙ",
    "ayat_bahasa": "dan bulan pun telah hilang cahayanya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075008.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5560,
    "ayat_number": 9,
    "ayat_arabic": "وَجُمِعَ الشَّمْسُ وَالْقَمَرُۙ",
    "ayat_bahasa": "lalu matahari dan bulan dikumpulkan, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075009.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5561,
    "ayat_number": 10,
    "ayat_arabic": "يَقُوْلُ الْاِنْسَانُ يَوْمَىِٕذٍ اَيْنَ الْمَفَرُّۚ",
    "ayat_bahasa": "pada hari itu manusia berkata, “Ke mana tempat lari?”",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075010.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5562,
    "ayat_number": 11,
    "ayat_arabic": "كَلَّا لَا وَزَرَۗ",
    "ayat_bahasa": "Tidak! Tidak ada tempat berlindung!",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075011.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5563,
    "ayat_number": 12,
    "ayat_arabic": "اِلٰى رَبِّكَ يَوْمَىِٕذِ ِۨالْمُسْتَقَرُّۗ",
    "ayat_bahasa": "Hanya kepada Tuhanmu tempat kembali pada hari itu.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075012.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5564,
    "ayat_number": 13,
    "ayat_arabic": "يُنَبَّؤُا الْاِنْسَانُ يَوْمَىِٕذٍۢ بِمَا قَدَّمَ وَاَخَّرَۗ",
    "ayat_bahasa": "Pada hari itu diberitakan kepada manusia apa yang telah dikerjakannya dan apa yang dilalaikannya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075013.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5565,
    "ayat_number": 14,
    "ayat_arabic": "بَلِ الْاِنْسَانُ عَلٰى نَفْسِهٖ بَصِيْرَةٌۙ",
    "ayat_bahasa": "Bahkan manusia menjadi saksi atas dirinya sendiri,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075014.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5566,
    "ayat_number": 15,
    "ayat_arabic": "وَّلَوْ اَلْقٰى مَعَاذِيْرَهٗۗ",
    "ayat_bahasa": "dan meskipun dia mengemukakan alasan-alasannya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075015.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5567,
    "ayat_number": 16,
    "ayat_arabic": "لَا تُحَرِّكْ بِهٖ لِسَانَكَ لِتَعْجَلَ بِهٖۗ",
    "ayat_bahasa": "Jangan engkau (Muhammad) gerakkan lidahmu (untuk membaca Al-Qur'an) karena hendak cepat-cepat (menguasai)nya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075016.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5568,
    "ayat_number": 17,
    "ayat_arabic": "اِنَّ عَلَيْنَا جَمْعَهٗ وَقُرْاٰنَهٗ ۚ",
    "ayat_bahasa": "Sesungguhnya Kami yang akan mengumpulkannya (di dadamu) dan membacakannya. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075017.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5569,
    "ayat_number": 18,
    "ayat_arabic": "فَاِذَا قَرَأْنٰهُ فَاتَّبِعْ قُرْاٰنَهٗ ۚ",
    "ayat_bahasa": "Apabila Kami telah selesai membacakannya maka ikutilah bacaannya itu. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075018.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5570,
    "ayat_number": 19,
    "ayat_arabic": "ثُمَّ اِنَّ عَلَيْنَا بَيَانَهٗ ۗ",
    "ayat_bahasa": "Kemudian sesungguhnya Kami yang akan menjelaskannya. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075019.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 577
  },
  {
    "ayat_id": 5571,
    "ayat_number": 20,
    "ayat_arabic": "كَلَّا بَلْ تُحِبُّوْنَ الْعَاجِلَةَۙ",
    "ayat_bahasa": "Tidak! Bahkan kamu mencintai kehidupan dunia, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075020.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5572,
    "ayat_number": 21,
    "ayat_arabic": "وَتَذَرُوْنَ الْاٰخِرَةَۗ",
    "ayat_bahasa": "dan mengabaikan (kehidupan) akhirat. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075021.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5573,
    "ayat_number": 22,
    "ayat_arabic": "وُجُوْهٌ يَّوْمَىِٕذٍ نَّاضِرَةٌۙ",
    "ayat_bahasa": "Wajah-wajah (orang mukmin) pada hari itu berseri-seri,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075022.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5574,
    "ayat_number": 23,
    "ayat_arabic": "اِلٰى رَبِّهَا نَاظِرَةٌ ۚ",
    "ayat_bahasa": "memandang Tuhannya. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075023.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5575,
    "ayat_number": 24,
    "ayat_arabic": "وَوُجُوْهٌ يَّوْمَىِٕذٍۢ بَاسِرَةٌۙ",
    "ayat_bahasa": "Dan wajah-wajah (orang kafir) pada hari itu muram, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075024.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5576,
    "ayat_number": 25,
    "ayat_arabic": "تَظُنُّ اَنْ يُّفْعَلَ بِهَا فَاقِرَةٌ ۗ",
    "ayat_bahasa": "mereka yakin bahwa akan ditimpakan kepadanya malapetaka yang sangat dahsyat.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075025.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5577,
    "ayat_number": 26,
    "ayat_arabic": "كَلَّآ اِذَا بَلَغَتِ التَّرَاقِيَۙ",
    "ayat_bahasa": "Tidak! Apabila (nyawa) telah sampai ke kerongkongan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075026.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5578,
    "ayat_number": 27,
    "ayat_arabic": "وَقِيْلَ مَنْ ۜرَاقٍۙ",
    "ayat_bahasa": "dan dikatakan (kepadanya), “Siapa yang dapat menyembuhkan?”",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075027.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5579,
    "ayat_number": 28,
    "ayat_arabic": "وَّظَنَّ اَنَّهُ الْفِرَاقُۙ",
    "ayat_bahasa": "Dan dia yakin bahwa itulah waktu perpisahan (dengan dunia), ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075028.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5580,
    "ayat_number": 29,
    "ayat_arabic": "وَالْتَفَّتِ السَّاقُ بِالسَّاقِۙ",
    "ayat_bahasa": "dan bertaut betis (kiri) dengan betis (kanan),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075029.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5581,
    "ayat_number": 30,
    "ayat_arabic": "اِلٰى رَبِّكَ يَوْمَىِٕذِ ِۨالْمَسَاقُ ۗ ࣖ",
    "ayat_bahasa": "kepada Tuhanmulah pada hari itu kamu dihalau. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075030.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5582,
    "ayat_number": 31,
    "ayat_arabic": "فَلَا صَدَّقَ وَلَا صَلّٰىۙ",
    "ayat_bahasa": "Karena dia (dahulu) tidak mau membenarkan (Al-Qur'an dan Rasul) dan tidak mau melaksanakan salat, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075031.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5583,
    "ayat_number": 32,
    "ayat_arabic": "وَلٰكِنْ كَذَّبَ وَتَوَلّٰىۙ",
    "ayat_bahasa": "tetapi justru dia mendustakan (Rasul) dan berpaling (dari kebenaran), ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075032.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5584,
    "ayat_number": 33,
    "ayat_arabic": "ثُمَّ ذَهَبَ اِلٰٓى اَهْلِهٖ يَتَمَطّٰىۗ",
    "ayat_bahasa": "kemudian dia pergi kepada keluarganya dengan sombong.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075033.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5585,
    "ayat_number": 34,
    "ayat_arabic": "اَوْلٰى لَكَ فَاَوْلٰىۙ",
    "ayat_bahasa": "Celakalah kamu! Maka celakalah! ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075034.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5586,
    "ayat_number": 35,
    "ayat_arabic": "ثُمَّ اَوْلٰى لَكَ فَاَوْلٰىۗ",
    "ayat_bahasa": "Sekali lagi, celakalah kamu (manusia)! Maka celakalah! ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075035.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5587,
    "ayat_number": 36,
    "ayat_arabic": "اَيَحْسَبُ الْاِنْسَانُ اَنْ يُّتْرَكَ سُدًىۗ",
    "ayat_bahasa": "Apakah manusia mengira, dia akan dibiarkan begitu saja (tanpa pertanggungjawaban)?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075036.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5588,
    "ayat_number": 37,
    "ayat_arabic": "اَلَمْ يَكُ نُطْفَةً مِّنْ مَّنِيٍّ يُّمْنٰى ",
    "ayat_bahasa": "Bukankah dia mulanya hanya setetes mani yang ditumpahkan (ke dalam rahim), ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075037.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5589,
    "ayat_number": 38,
    "ayat_arabic": "ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوّٰىۙ",
    "ayat_bahasa": "kemudian (mani itu) menjadi sesuatu yang melekat, lalu Allah menciptakannya dan menyempurnakannya, ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075038.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5590,
    "ayat_number": 39,
    "ayat_arabic": "فَجَعَلَ مِنْهُ الزَّوْجَيْنِ الذَّكَرَ وَالْاُنْثٰىۗ",
    "ayat_bahasa": "lalu Dia menjadikan darinya sepasang laki-laki dan perempuan. ",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075039.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  },
  {
    "ayat_id": 5591,
    "ayat_number": 40,
    "ayat_arabic": "اَلَيْسَ ذٰلِكَ بِقٰدِرٍ عَلٰٓى اَنْ يُّحْيِ َۧ الْمَوْتٰى ࣖ",
    "ayat_bahasa": "Bukankah (Allah yang berbuat) demikian berkuasa (pula) menghidupkan orang mati?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/075040.mp3",
    "surat_id": 75,
    "juz_id": 29,
    "page_number": 578
  }
]
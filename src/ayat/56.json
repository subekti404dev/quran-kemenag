[
  {
    "ayat_id": 4980,
    "ayat_number": 1,
    "ayat_arabic": "اِذَا وَقَعَتِ الْوَاقِعَةُۙ ",
    "ayat_bahasa": "Apabila terjadi hari Kiamat,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056001.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 534
  },
  {
    "ayat_id": 4981,
    "ayat_number": 2,
    "ayat_arabic": "لَيْسَ لِوَقْعَتِهَا كَاذِبَةٌ  ۘ ",
    "ayat_bahasa": "terjadinya tidak dapat didustakan (disangkal).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056002.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 534
  },
  {
    "ayat_id": 4982,
    "ayat_number": 3,
    "ayat_arabic": "خَافِضَةٌ رَّافِعَةٌ ",
    "ayat_bahasa": "(Kejadian itu) merendahkan (satu golongan) dan meninggikan (golongan yang lain).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056003.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 534
  },
  {
    "ayat_id": 4983,
    "ayat_number": 4,
    "ayat_arabic": "اِذَا رُجَّتِ الْاَرْضُ رَجًّاۙ ",
    "ayat_bahasa": "Apabila bumi diguncangkan sedahsyat-dahsyatnya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056004.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 534
  },
  {
    "ayat_id": 4984,
    "ayat_number": 5,
    "ayat_arabic": "وَّبُسَّتِ الْجِبَالُ بَسًّاۙ  ",
    "ayat_bahasa": "dan gunung-gunung dihancurluluhkan sehancur-hancurnya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056005.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 534
  },
  {
    "ayat_id": 4985,
    "ayat_number": 6,
    "ayat_arabic": "فَكَانَتْ هَبَاۤءً مُّنْۢبَثًّاۙ ",
    "ayat_bahasa": "maka jadilah ia debu yang beterbangan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056006.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 534
  },
  {
    "ayat_id": 4986,
    "ayat_number": 7,
    "ayat_arabic": "وَّكُنْتُمْ اَزْوَاجًا ثَلٰثَةً ۗ",
    "ayat_bahasa": "dan kamu menjadi tiga golongan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056007.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 534
  },
  {
    "ayat_id": 4987,
    "ayat_number": 8,
    "ayat_arabic": "فَاَصْحٰبُ الْمَيْمَنَةِ ەۙ مَآ اَصْحٰبُ الْمَيْمَنَةِ ۗ",
    "ayat_bahasa": "yaitu golongan kanan, alangkah mulianya golongan kanan itu,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056008.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 534
  },
  {
    "ayat_id": 4988,
    "ayat_number": 9,
    "ayat_arabic": "وَاَصْحٰبُ الْمَشْـَٔمَةِ ەۙ مَآ اَصْحٰبُ الْمَشْـَٔمَةِ ۗ",
    "ayat_bahasa": "dan golongan kiri, alangkah sengsaranya golongan kiri itu,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056009.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 534
  },
  {
    "ayat_id": 4989,
    "ayat_number": 10,
    "ayat_arabic": "وَالسّٰبِقُوْنَ السّٰبِقُوْنَۙ  ",
    "ayat_bahasa": "dan orang-orang yang paling dahulu (beriman), merekalah yang paling dahulu (masuk surga).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056010.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 534
  },
  {
    "ayat_id": 4990,
    "ayat_number": 11,
    "ayat_arabic": "اُولٰۤىِٕكَ الْمُقَرَّبُوْنَۚ ",
    "ayat_bahasa": "Mereka itulah orang yang dekat (kepada Allah),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056011.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 534
  },
  {
    "ayat_id": 4991,
    "ayat_number": 12,
    "ayat_arabic": "فِيْ جَنّٰتِ النَّعِيْمِ ",
    "ayat_bahasa": "Berada dalam surga kenikmatan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056012.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 534
  },
  {
    "ayat_id": 4992,
    "ayat_number": 13,
    "ayat_arabic": "ثُلَّةٌ مِّنَ الْاَوَّلِيْنَۙ  ",
    "ayat_bahasa": "segolongan besar dari orang-orang yang terdahulu,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056013.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 534
  },
  {
    "ayat_id": 4993,
    "ayat_number": 14,
    "ayat_arabic": "وَقَلِيْلٌ مِّنَ الْاٰخِرِيْنَۗ  ",
    "ayat_bahasa": "dan segolongan kecil dari orang-orang yang kemudian.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056014.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 534
  },
  {
    "ayat_id": 4994,
    "ayat_number": 15,
    "ayat_arabic": "عَلٰى سُرُرٍ مَّوْضُوْنَةٍۙ  ",
    "ayat_bahasa": "Mereka berada di atas dipan-dipan yang bertahtakan emas dan permata,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056015.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 534
  },
  {
    "ayat_id": 4995,
    "ayat_number": 16,
    "ayat_arabic": "مُّتَّكِـِٕيْنَ عَلَيْهَا مُتَقٰبِلِيْنَ",
    "ayat_bahasa": "mereka bersandar di atasnya berhadap-hadapan.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056016.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 534
  },
  {
    "ayat_id": 4996,
    "ayat_number": 17,
    "ayat_arabic": "يَطُوْفُ عَلَيْهِمْ وِلْدَانٌ مُّخَلَّدُوْنَۙ  ",
    "ayat_bahasa": "Mereka dikelilingi oleh anak-anak muda yang tetap muda,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056017.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 4997,
    "ayat_number": 18,
    "ayat_arabic": "بِاَكْوَابٍ وَّاَبَارِيْقَۙ وَكَأْسٍ مِّنْ مَّعِيْنٍۙ  ",
    "ayat_bahasa": "dengan membawa gelas, cerek dan sloki (piala) berisi minuman yang diambil dari air yang mengalir,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056018.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 4998,
    "ayat_number": 19,
    "ayat_arabic": "لَّا يُصَدَّعُوْنَ عَنْهَا وَلَا يُنْزِفُوْنَۙ  ",
    "ayat_bahasa": "mereka tidak pening karenanya dan tidak pula mabuk,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056019.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 4999,
    "ayat_number": 20,
    "ayat_arabic": "وَفَاكِهَةٍ مِّمَّا يَتَخَيَّرُوْنَۙ  ",
    "ayat_bahasa": "dan buah-buahan apa pun yang mereka pilih,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056020.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5000,
    "ayat_number": 21,
    "ayat_arabic": "وَلَحْمِ طَيْرٍ مِّمَّا يَشْتَهُوْنَۗ  ",
    "ayat_bahasa": "dan daging burung apa pun yang mereka inginkan.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056021.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5001,
    "ayat_number": 22,
    "ayat_arabic": "وَحُوْرٌ عِيْنٌۙ  ",
    "ayat_bahasa": "Dan ada bidadari-bidadari yang bermata indah,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056022.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5002,
    "ayat_number": 23,
    "ayat_arabic": "كَاَمْثَالِ اللُّؤْلُؤِ الْمَكْنُوْنِۚ  ",
    "ayat_bahasa": "laksana mutiara yang tersimpan baik.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056023.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5003,
    "ayat_number": 24,
    "ayat_arabic": "جَزَاۤءًۢ بِمَا كَانُوْا يَعْمَلُوْنَ",
    "ayat_bahasa": "Sebagai balasan atas apa yang mereka kerjakan.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056024.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5004,
    "ayat_number": 25,
    "ayat_arabic": "لَا يَسْمَعُوْنَ فِيْهَا لَغْوًا وَّلَا تَأْثِيْمًاۙ  ",
    "ayat_bahasa": "Di sana mereka tidak mendengar percakapan yang sia-sia maupun yang menimbulkan dosa,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056025.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5005,
    "ayat_number": 26,
    "ayat_arabic": "اِلَّا قِيْلًا سَلٰمًا سَلٰمًا ",
    "ayat_bahasa": "tetapi mereka mendengar ucapan salam.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056026.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5006,
    "ayat_number": 27,
    "ayat_arabic": "وَاَصْحٰبُ الْيَمِينِ ەۙ مَآ اَصْحٰبُ الْيَمِيْنِۗ",
    "ayat_bahasa": "Dan golongan kanan, siapakah golongan kanan itu.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056027.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5007,
    "ayat_number": 28,
    "ayat_arabic": "فِيْ سِدْرٍ مَّخْضُوْدٍۙ  ",
    "ayat_bahasa": "(Mereka) berada di antara pohon bidara yang tidak berduri,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056028.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5008,
    "ayat_number": 29,
    "ayat_arabic": "وَّطَلْحٍ مَّنْضُوْدٍۙ  ",
    "ayat_bahasa": "dan pohon pisang yang bersusun-susun (buahnya),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056029.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5009,
    "ayat_number": 30,
    "ayat_arabic": "وَّظِلٍّ مَّمْدُوْدٍۙ  ",
    "ayat_bahasa": "dan naungan yang terbentang luas,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056030.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5010,
    "ayat_number": 31,
    "ayat_arabic": "وَّمَاۤءٍ مَّسْكُوْبٍۙ  ",
    "ayat_bahasa": "dan air yang mengalir terus-menerus,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056031.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5011,
    "ayat_number": 32,
    "ayat_arabic": "وَّفَاكِهَةٍ كَثِيْرَةٍۙ  ",
    "ayat_bahasa": "dan buah-buahan yang banyak,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056032.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5012,
    "ayat_number": 33,
    "ayat_arabic": "لَّا مَقْطُوْعَةٍ وَّلَا مَمْنُوْعَةٍۙ  ",
    "ayat_bahasa": "yang tidak berhenti berbuah dan tidak terlarang mengambilnya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056033.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5013,
    "ayat_number": 34,
    "ayat_arabic": "وَّفُرُشٍ مَّرْفُوْعَةٍۗ  ",
    "ayat_bahasa": "dan kasur-kasur yang tebal lagi empuk.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056034.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5014,
    "ayat_number": 35,
    "ayat_arabic": "اِنَّآ اَنْشَأْنٰهُنَّ اِنْشَاۤءًۙ  ",
    "ayat_bahasa": "Kami menciptakan mereka (bidadari-bidadari itu) secara langsung,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056035.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5015,
    "ayat_number": 36,
    "ayat_arabic": "فَجَعَلْنٰهُنَّ اَبْكَارًاۙ  ",
    "ayat_bahasa": "lalu Kami jadikan mereka perawan-perawan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056036.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5016,
    "ayat_number": 37,
    "ayat_arabic": "عُرُبًا اَتْرَابًاۙ  ",
    "ayat_bahasa": "yang penuh cinta (dan) sebaya umurnya,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056037.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5017,
    "ayat_number": 38,
    "ayat_arabic": "لِّاَصْحٰبِ الْيَمِيْنِۗ  ࣖ ",
    "ayat_bahasa": "untuk golongan kanan,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056038.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5018,
    "ayat_number": 39,
    "ayat_arabic": "ثُلَّةٌ مِّنَ الْاَوَّلِيْنَۙ  ",
    "ayat_bahasa": "segolongan besar dari orang-orang yang terdahulu,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056039.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5019,
    "ayat_number": 40,
    "ayat_arabic": "وَثُلَّةٌ مِّنَ الْاٰخِرِيْنَۗ  ",
    "ayat_bahasa": "dan segolongan besar pula dari orang yang kemudian.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056040.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5020,
    "ayat_number": 41,
    "ayat_arabic": "وَاَصْحٰبُ الشِّمَالِ ەۙ مَآ اَصْحٰبُ الشِّمَالِۗ",
    "ayat_bahasa": "Dan golongan kiri, alangkah sengsaranya golongan kiri itu.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056041.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5021,
    "ayat_number": 42,
    "ayat_arabic": "فِيْ سَمُوْمٍ وَّحَمِيْمٍۙ  ",
    "ayat_bahasa": "(Mereka) dalam siksaan angin yang sangat panas dan air yang mendidih,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056042.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5022,
    "ayat_number": 43,
    "ayat_arabic": "وَّظِلٍّ مِّنْ يَّحْمُوْمٍۙ  ",
    "ayat_bahasa": "dan naungan asap yang hitam,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056043.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5023,
    "ayat_number": 44,
    "ayat_arabic": "لَّا بَارِدٍ وَّلَا كَرِيْمٍ ",
    "ayat_bahasa": "tidak sejuk dan tidak menyenangkan.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056044.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5024,
    "ayat_number": 45,
    "ayat_arabic": "اِنَّهُمْ كَانُوْا قَبْلَ ذٰلِكَ مُتْرَفِيْنَۚ ",
    "ayat_bahasa": "Sesungguhnya mereka sebelum itu (dahulu) hidup bermewah-mewah,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056045.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5025,
    "ayat_number": 46,
    "ayat_arabic": "وَكَانُوْا يُصِرُّوْنَ عَلَى الْحِنْثِ الْعَظِيْمِۚ  ",
    "ayat_bahasa": "dan mereka terus-menerus mengerjakan dosa yang besar,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056046.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5026,
    "ayat_number": 47,
    "ayat_arabic": "وَكَانُوْا يَقُوْلُوْنَ ەۙ اَىِٕذَا مِتْنَا وَكُنَّا تُرَابًا وَّعِظَامًا ءَاِنَّا لَمَبْعُوْثُوْنَۙ",
    "ayat_bahasa": "dan mereka berkata, “Apabila kami sudah mati, menjadi tanah dan tulang-belulang, apakah kami benar-benar akan dibangkitkan kembali?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056047.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5027,
    "ayat_number": 48,
    "ayat_arabic": "اَوَاٰبَاۤؤُنَا الْاَوَّلُوْنَ ",
    "ayat_bahasa": "Apakah nenek moyang kami yang terdahulu (dibangkitkan pula)?”",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056048.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5028,
    "ayat_number": 49,
    "ayat_arabic": "قُلْ اِنَّ الْاَوَّلِيْنَ وَالْاٰخِرِيْنَۙ ",
    "ayat_bahasa": "Katakanlah, “(Ya), sesungguhnya orang-orang yang terdahulu dan yang kemudian,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056049.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5029,
    "ayat_number": 50,
    "ayat_arabic": "لَمَجْمُوْعُوْنَۙ اِلٰى مِيْقَاتِ يَوْمٍ مَّعْلُوْمٍ ",
    "ayat_bahasa": "pasti semua akan dikumpulkan pada waktu tertentu, pada hari yang sudah dimaklumi.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056050.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 535
  },
  {
    "ayat_id": 5030,
    "ayat_number": 51,
    "ayat_arabic": "ثُمَّ اِنَّكُمْ اَيُّهَا الضَّاۤ لُّوْنَ الْمُكَذِّبُوْنَۙ  ",
    "ayat_bahasa": "Kemudian sesungguhnya kamu, wahai orang-orang yang sesat lagi mendustakan!",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056051.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5031,
    "ayat_number": 52,
    "ayat_arabic": "لَاٰكِلُوْنَ مِنْ شَجَرٍ مِّنْ زَقُّوْمٍۙ  ",
    "ayat_bahasa": "pasti akan memakan pohon zaqqum,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056052.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5032,
    "ayat_number": 53,
    "ayat_arabic": "فَمَالِـُٔوْنَ مِنْهَا الْبُطُوْنَۚ  ",
    "ayat_bahasa": "maka akan penuh perutmu dengannya.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056053.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5033,
    "ayat_number": 54,
    "ayat_arabic": "فَشَارِبُوْنَ عَلَيْهِ مِنَ الْحَمِيْمِۚ  ",
    "ayat_bahasa": "Setelah itu kamu akan meminum air yang sangat panas.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056054.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5034,
    "ayat_number": 55,
    "ayat_arabic": "فَشَارِبُوْنَ شُرْبَ الْهِيْمِۗ  ",
    "ayat_bahasa": "Maka kamu minum seperti unta (yang sangat haus) minum.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056055.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5035,
    "ayat_number": 56,
    "ayat_arabic": "هٰذَا نُزُلُهُمْ يَوْمَ الدِّيْنِۗ  ",
    "ayat_bahasa": "Itulah hidangan untuk mereka pada hari pembalasan.”",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056056.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5036,
    "ayat_number": 57,
    "ayat_arabic": "نَحْنُ خَلَقْنٰكُمْ فَلَوْلَا تُصَدِّقُوْنَ ",
    "ayat_bahasa": "Kami telah menciptakan kamu, mengapa kamu tidak membenarkan (hari berbangkit)?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056057.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5037,
    "ayat_number": 58,
    "ayat_arabic": "اَفَرَءَيْتُمْ مَّا تُمْنُوْنَۗ  ",
    "ayat_bahasa": "Maka adakah kamu perhatikan, tentang (benih manusia) yang kamu pancarkan.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056058.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5038,
    "ayat_number": 59,
    "ayat_arabic": "ءَاَنْتُمْ تَخْلُقُوْنَهٗٓ اَمْ نَحْنُ الْخَالِقُوْنَ ",
    "ayat_bahasa": "Kamukah yang menciptakannya, ataukah Kami penciptanya?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056059.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5039,
    "ayat_number": 60,
    "ayat_arabic": "نَحْنُ قَدَّرْنَا بَيْنَكُمُ الْمَوْتَ وَمَا نَحْنُ بِمَسْبُوْقِيْنَۙ  ",
    "ayat_bahasa": "Kami telah menentukan kematian masing-masing kamu dan Kami tidak lemah,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056060.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5040,
    "ayat_number": 61,
    "ayat_arabic": "عَلٰٓى اَنْ نُّبَدِّلَ اَمْثَالَكُمْ وَنُنْشِئَكُمْ فِيْ مَا لَا تَعْلَمُوْنَ",
    "ayat_bahasa": "untuk menggantikan kamu dengan orang-orang yang seperti kamu (di dunia) dan membangkitkan kamu kelak (di akhirat) dalam keadaan yang tidak kamu ketahui.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056061.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5041,
    "ayat_number": 62,
    "ayat_arabic": "وَلَقَدْ عَلِمْتُمُ النَّشْاَةَ الْاُوْلٰى فَلَوْلَا تَذَكَّرُوْنَ ",
    "ayat_bahasa": "Dan sungguh, kamu telah tahu penciptaan yang pertama, mengapa kamu tidak mengambil pelajaran?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056062.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5042,
    "ayat_number": 63,
    "ayat_arabic": "اَفَرَءَيْتُمْ مَّا تَحْرُثُوْنَۗ  ",
    "ayat_bahasa": "Pernahkah kamu perhatikan benih yang kamu tanam?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056063.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5043,
    "ayat_number": 64,
    "ayat_arabic": "ءَاَنْتُمْ تَزْرَعُوْنَهٗٓ اَمْ نَحْنُ الزَّارِعُوْنَ ",
    "ayat_bahasa": "Kamukah yang menumbuhkannya ataukah Kami yang menumbuhkan?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056064.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5044,
    "ayat_number": 65,
    "ayat_arabic": "لَوْ نَشَاۤءُ لَجَعَلْنٰهُ حُطَامًا فَظَلْتُمْ تَفَكَّهُوْنَۙ  ",
    "ayat_bahasa": "Sekiranya Kami kehendaki, niscaya Kami hancurkan sampai lumat; maka kamu akan heran tercengang,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056065.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5045,
    "ayat_number": 66,
    "ayat_arabic": "اِنَّا لَمُغْرَمُوْنَۙ  ",
    "ayat_bahasa": "(sambil berkata), “Sungguh, kami benar-benar menderita kerugian,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056066.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5046,
    "ayat_number": 67,
    "ayat_arabic": "بَلْ نَحْنُ مَحْرُوْمُوْنَ ",
    "ayat_bahasa": "bahkan kami tidak mendapat hasil apa pun.”",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056067.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5047,
    "ayat_number": 68,
    "ayat_arabic": "اَفَرَءَيْتُمُ الْمَاۤءَ الَّذِيْ تَشْرَبُوْنَۗ  ",
    "ayat_bahasa": "Pernahkah kamu memperhatikan air yang kamu minum?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056068.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5048,
    "ayat_number": 69,
    "ayat_arabic": "ءَاَنْتُمْ اَنْزَلْتُمُوْهُ مِنَ الْمُزْنِ اَمْ نَحْنُ الْمُنْزِلُوْنَ ",
    "ayat_bahasa": "Kamukah yang menurunkannya dari awan ataukah Kami yang menurunkan?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056069.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5049,
    "ayat_number": 70,
    "ayat_arabic": "لَوْ نَشَاۤءُ جَعَلْنٰهُ اُجَاجًا فَلَوْلَا تَشْكُرُوْنَ ",
    "ayat_bahasa": "Sekiranya Kami menghendaki, niscaya Kami menjadikannya asin, mengapa kamu tidak bersyukur?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056070.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5050,
    "ayat_number": 71,
    "ayat_arabic": "اَفَرَءَيْتُمُ النَّارَ الَّتِيْ تُوْرُوْنَۗ  ",
    "ayat_bahasa": "Maka pernahkah kamu memperhatikan tentang api yang kamu nyalakan (dengan kayu)?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056071.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5051,
    "ayat_number": 72,
    "ayat_arabic": "ءَاَنْتُمْ اَنْشَأْتُمْ شَجَرَتَهَآ اَمْ نَحْنُ الْمُنْشِـُٔوْنَ ",
    "ayat_bahasa": "Kamukah yang menumbuhkan kayu itu ataukah Kami yang menumbuhkan?",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056072.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5052,
    "ayat_number": 73,
    "ayat_arabic": "نَحْنُ جَعَلْنٰهَا تَذْكِرَةً وَّمَتَاعًا لِّلْمُقْوِيْنَۚ ",
    "ayat_bahasa": "Kami menjadikannya (api itu) untuk peringatan dan bahan yang berguna bagi musafir.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056073.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5053,
    "ayat_number": 74,
    "ayat_arabic": "فَسَبِّحْ بِاسْمِ رَبِّكَ الْعَظِيْمِ ࣖ ",
    "ayat_bahasa": "Maka bertasbihlah dengan (menyebut) nama Tuhanmu Yang Mahabesar.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056074.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5054,
    "ayat_number": 75,
    "ayat_arabic": "فَلَآ اُقْسِمُ بِمَوٰقِعِ النُّجُوْمِ ",
    "ayat_bahasa": "Lalu Aku bersumpah dengan tempat beredarnya bintang-bintang.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056075.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5055,
    "ayat_number": 76,
    "ayat_arabic": "وَاِنَّهٗ لَقَسَمٌ لَّوْ تَعْلَمُوْنَ عَظِيْمٌۙ  ",
    "ayat_bahasa": "Dan sesungguhnya itu benar-benar sumpah yang besar sekiranya kamu mengetahui,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056076.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 536
  },
  {
    "ayat_id": 5056,
    "ayat_number": 77,
    "ayat_arabic": "اِنَّهٗ لَقُرْاٰنٌ كَرِيْمٌۙ  ",
    "ayat_bahasa": "dan (ini) sesungguhnya Al-Qur'an yang sangat mulia,",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056077.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 537
  },
  {
    "ayat_id": 5057,
    "ayat_number": 78,
    "ayat_arabic": "فِيْ كِتٰبٍ مَّكْنُوْنٍۙ ",
    "ayat_bahasa": "dalam Kitab yang terpelihara (Lauh Mahfuzh),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056078.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 537
  },
  {
    "ayat_id": 5058,
    "ayat_number": 79,
    "ayat_arabic": "لَّا يَمَسُّهٗٓ اِلَّا الْمُطَهَّرُوْنَۙ  ",
    "ayat_bahasa": "tidak ada yang menyentuhnya selain hamba-hamba yang disucikan.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056079.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 537
  },
  {
    "ayat_id": 5059,
    "ayat_number": 80,
    "ayat_arabic": "تَنْزِيْلٌ مِّنْ رَّبِّ الْعٰلَمِيْنَ ",
    "ayat_bahasa": "Diturunkan dari Tuhan seluruh alam.",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056080.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 537
  },
  {
    "ayat_id": 5060,
    "ayat_number": 81,
    "ayat_arabic": "اَفَبِهٰذَا الْحَدِيْثِ اَنْتُمْ مُّدْهِنُوْنَ ",
    "ayat_bahasa": "Apakah kamu menganggap remeh berita ini (Al-Qur'an),",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056081.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 537
  },
  {
    "ayat_id": 5061,
    "ayat_number": 82,
    "ayat_arabic": "وَتَجْعَلُوْنَ رِزْقَكُمْ اَنَّكُمْ تُكَذِّبُوْنَ ",
    "ayat_bahasa": "dan kamu menjadikan rezeki yang kamu terima (dari Allah) justru untuk mendustakan(-Nya).",
    "ayat_sound": "https://quran.kemenag.go.id/cmsq/source/s01/056082.mp3",
    "surat_id": 56,
    "juz_id": 27,
    "page_number": 537
  },
  {
